{
  if (node.isLeaf()) {
    Image img=new Image(NODE_IMAGE_NAME,getImage("node.gif"));
    return img;
  }
 else {
    return new Image(NODE_IMAGE_NAME,(SharedResource)null){
      protected Resource getImageResource(){
        if (isExpanded(node)) {
          return getImage("folderopen.gif");
        }
 else {
          return getImage("folder.gif");
        }
      }
    }
;
  }
}
