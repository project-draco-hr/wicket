{
  if (node.isLeaf()) {
    Image img=new Image(NODE_IMAGE_NAME,"node.gif");
    return img;
  }
 else {
    if (isExpanded(node)) {
      Image img=new Image(NODE_IMAGE_NAME,"folderopen.gif");
      return img;
    }
 else {
      Image img=new Image(NODE_IMAGE_NAME,"folder.gif");
      return img;
    }
  }
}
