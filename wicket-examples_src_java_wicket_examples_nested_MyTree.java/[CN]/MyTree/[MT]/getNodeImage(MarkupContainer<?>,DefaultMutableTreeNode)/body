{
  if (node.isLeaf()) {
    Image img=new Image(parent,NODE_IMAGE_NAME,nodeImage);
    return img;
  }
 else {
    return new Image(parent,NODE_IMAGE_NAME){
      protected ResourceReference getImageResourceReference(){
        if (isExpanded(node)) {
          return folderOpen;
        }
 else {
          return folder;
        }
      }
    }
;
  }
}
