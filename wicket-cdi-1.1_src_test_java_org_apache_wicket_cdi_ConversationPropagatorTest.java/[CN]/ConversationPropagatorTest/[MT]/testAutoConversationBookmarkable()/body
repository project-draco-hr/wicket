{
  configure(new CdiConfiguration());
  tester.startPage(TestConversationalPage.class,new PageParameters().add("pageType","bookmarkable",INamedParameters.Type.MANUAL));
  int i;
  for (i=0; i < 3; i++) {
    tester.assertCount(i);
    tester.clickLink("increment");
  }
  tester.clickLink("next");
  for (i=0; i < 3; i++) {
    tester.clickLink("increment");
    tester.assertCount(1);
  }
}
