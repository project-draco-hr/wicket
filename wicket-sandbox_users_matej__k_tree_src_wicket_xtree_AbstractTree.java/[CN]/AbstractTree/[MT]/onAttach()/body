{
  if (attached == false) {
    checkModel();
    if (dirtyAll && rootItem != null) {
      clearAllItem();
    }
 else {
      rebuildDirty();
    }
    if (rootItem == null) {
      TreeNode rootNode=(TreeNode)getModelObject().getRoot();
      if (rootNode != null) {
        if (isRootLess()) {
          rootItem=createTreeItem(rootNode,-1);
          rootItem.setVisible(false);
        }
 else {
          rootItem=createTreeItem(rootNode,0);
        }
        buildItemChildren(rootItem);
      }
    }
    attached=true;
  }
}
