{
  if (!isMutable) {
    return text;
  }
 else {
    final StringBuffer buffer=new StringBuffer();
    buffer.append('<');
    if (isClose && !isOpen) {
      buffer.append('/');
    }
    buffer.append(name);
    if (attributes.size() > 0) {
      buffer.append(' ');
      buffer.append(attributes);
    }
    if (isClose && isOpen) {
      buffer.append('/');
    }
    buffer.append('>');
    return buffer.toString();
  }
}
