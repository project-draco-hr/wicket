{
  if (target != null) {
    final IModel<T> model=getProvider().model(t);
    visitChildren(BranchItem.class,new IVisitor<BranchItem<T>,Void>(){
      public void component(      BranchItem<T> branch,      IVisit<Void> visit){
        if (model.equals(branch.getModel())) {
          target.add(branch);
          visit.stop();
        }
      }
    }
);
    model.detach();
  }
}
