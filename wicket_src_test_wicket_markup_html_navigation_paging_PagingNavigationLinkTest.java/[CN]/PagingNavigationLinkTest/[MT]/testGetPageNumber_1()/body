{
  PagingNavigationLink link=new PagingNavigationLink(new MockPageWithOneComponent(),"component",mock,0);
  currentpage=0;
  pagecount=0;
  assertTrue("is first",link.isFirst());
  assertFalse("is last",link.isLast());
  assertEquals(0,link.getPageNumber());
  pagecount=1;
  assertTrue("is first",link.isFirst());
  assertTrue("is last",link.isLast());
  assertEquals(0,link.getPageNumber());
  pagecount=2;
  assertTrue("is first",link.isFirst());
  assertFalse("is last",link.isLast());
  assertEquals(0,link.getPageNumber());
}
