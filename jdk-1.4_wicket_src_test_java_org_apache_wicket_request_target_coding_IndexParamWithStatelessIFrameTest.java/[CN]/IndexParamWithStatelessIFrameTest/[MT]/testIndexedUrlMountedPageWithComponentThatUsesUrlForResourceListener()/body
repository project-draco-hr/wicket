{
  tester.getApplication().mount(new IndexedParamUrlCodingStrategy("/test",TestPageWithIFrame.class));
  TestPageWithIFrame test=new TestPageWithIFrame(new PageParameters("0=foo,1=bar"));
  tester.startPage(test);
  tester.assertRenderedPage(TestPageWithIFrame.class);
  tester.assertNoErrorMessage();
  tester.assertContains("src=\"test/wicket:interface/%3A0%3Aframe%3A%3AIResourceListener%3A%3A/\"");
}
