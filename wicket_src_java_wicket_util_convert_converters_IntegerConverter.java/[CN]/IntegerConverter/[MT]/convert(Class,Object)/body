{
  if (value == null) {
    return null;
  }
  if (value instanceof Integer) {
    return (value);
  }
 else   if (value instanceof Number) {
    return new Integer(((Number)value).intValue());
  }
  try {
    return (new Integer(value.toString()));
  }
 catch (  Exception e) {
    throw new ConversionException(e);
  }
}
