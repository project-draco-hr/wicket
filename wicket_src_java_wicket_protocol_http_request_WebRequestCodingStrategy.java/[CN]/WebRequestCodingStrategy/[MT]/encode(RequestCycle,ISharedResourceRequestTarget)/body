{
  String prefix=urlPrefix(requestCycle);
  String resourceKey=requestTarget.getResourceKey();
  if ((resourceKey == null) || (resourceKey.trim().length() == 0)) {
    return prefix;
  }
 else {
    if (prefix.endsWith("/") || resourceKey.startsWith("/")) {
      return prefix + resourceKey;
    }
    return prefix + "/" + resourceKey;
  }
}
