{
  final Map map=super.messageModel(formComponent);
  if (checkMin) {
    map.put("min",new Long(min));
  }
  if (checkMax) {
    map.put("max",new Long(max));
  }
  int size=0;
  if (formComponent.getInput() != null) {
    size=formComponent.getInput().length();
  }
  map.put("length",new Integer(size));
  return map;
}
