{
  final ComponentTag tag=nextComponentTag();
  if (tag == null) {
    return tag;
  }
  if (ignoreTheRest == true) {
    return tag;
  }
  if (tag.isHeadTag()) {
    ignoreTheRest=true;
    if (tag.getId() == null) {
      tag.setId(HEADER_ID);
    }
    tag.setInternalTag(true);
    return tag;
  }
 else   if (tag.isBodyTag()) {
    insertHeadTag();
    ignoreTheRest=true;
    return tag;
  }
  return tag;
}
