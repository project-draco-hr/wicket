{
  String strReturn="";
  String strSOAPPacket=getSOAPQuote(symbol);
  int nStart=strSOAPPacket.indexOf("<Result xsi:type='xsd:float'>");
  if (nStart == -1) {
    return "(unknown)";
  }
  int nEnd=strSOAPPacket.indexOf("</Result>");
  strReturn=strSOAPPacket.substring(nStart + 29,nEnd);
  return strReturn;
}
