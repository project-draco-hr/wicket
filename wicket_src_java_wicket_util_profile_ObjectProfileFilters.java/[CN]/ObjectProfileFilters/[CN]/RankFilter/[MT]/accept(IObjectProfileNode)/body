{
  final IObjectProfileNode parent=node.parent();
  if (parent == null) {
    return true;
  }
  final IObjectProfileNode[] siblings=parent.children();
  for (int r=0, rLimit=Math.min(siblings.length,m_threshold); r < rLimit; ++r) {
    if (siblings[r] == node) {
      return true;
    }
  }
  return false;
}
