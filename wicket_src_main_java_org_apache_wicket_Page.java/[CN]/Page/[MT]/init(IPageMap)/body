{
  if (isBookmarkable()) {
    setStatelessHint(true);
  }
  if (pageMap != null) {
    setPageMap(pageMap);
  }
 else {
    throw new IllegalStateException("PageMap cannot be null");
  }
  setNextAvailableId();
  setVersioned(Application.get().getPageSettings().getVersionPagesByDefault());
  dirty();
}
