{
  final Object o=get(key);
  if (o == null) {
    return null;
  }
 else   if (o.getClass().isArray() && Array.getLength(o) > 0) {
    final Object arrayValue=Array.get(o,0);
    if (arrayValue == null) {
      return null;
    }
 else {
      return arrayValue.toString();
    }
  }
 else {
    return o.toString();
  }
}
