{
  ComponentTag tag=(ComponentTag)getParent().nextTag();
  if (tag == null) {
    return tag;
  }
  if (tag.isOpen() && "body".equalsIgnoreCase(tag.getName()) && (tag.getNamespace() == null)) {
    if (tag.getId() == null) {
      tag.setId(HtmlBodyContainer.BODY_ID);
      tag.setAutoComponentTag(true);
      tag.setModified(true);
    }
  }
  return tag;
}
