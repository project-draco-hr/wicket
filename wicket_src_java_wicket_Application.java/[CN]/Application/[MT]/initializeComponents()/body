{
  try {
    for (Enumeration e=getClass().getClassLoader().getResources("META-INF/wicket-component-initializers.txt"); e.hasMoreElements(); ) {
      initializeComponents((URL)e.nextElement());
    }
    for (Enumeration e=getClass().getClassLoader().getResources("META-INF/" + getName() + "-wicket-component-initializers.txt"); e.hasMoreElements(); ) {
      initializeComponents((URL)e.nextElement());
    }
  }
 catch (  IOException e) {
    throw new WicketRuntimeException("Unable to load initializers file",e);
  }
}
