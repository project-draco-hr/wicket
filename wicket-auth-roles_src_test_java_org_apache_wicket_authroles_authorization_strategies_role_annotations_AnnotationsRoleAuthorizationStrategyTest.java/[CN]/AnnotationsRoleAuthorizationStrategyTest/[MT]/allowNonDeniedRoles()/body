{
  AnnotationsRoleAuthorizationStrategy strategy=new AnnotationsRoleAuthorizationStrategy(new IRoleCheckingStrategy(){
    @Override public boolean hasAnyRole(    Roles roles){
      return roles.contains("role1");
    }
  }
);
  TestComponent component=Mockito.mock(TestComponent.class);
  assertTrue(strategy.isActionAuthorized(component,Component.RENDER));
}
