{
  byte[] result=null;
  try {
    SecretKey key=generateKey();
    PBEParameterSpec spec=new PBEParameterSpec(salt,count);
    Cipher ciph=Cipher.getInstance(CRYPT_METHOD);
    ciph.init(mode,key,spec);
    result=ciph.doFinal(input);
  }
 catch (  Exception e) {
    return null;
  }
  return result;
}
