{
  if (field.isAnnotationPresent(SpringBean.class)) {
    SpringBean annot=field.getAnnotation(SpringBean.class);
    SpringBeanLocator locator=new SpringBeanLocator(annot.name(),field.getType(),contextLocator);
    Object cachedValue=cache.get(locator);
    if (cachedValue != null) {
      return cachedValue;
    }
    final Object target;
    if (wrapInProxies) {
      target=LazyInitProxyFactory.createProxy(field.getType(),locator);
    }
 else {
      target=locator.locateProxyTarget();
    }
    if (locator.isSingletonBean()) {
      cache.put(locator,target);
    }
    return target;
  }
  return null;
}
