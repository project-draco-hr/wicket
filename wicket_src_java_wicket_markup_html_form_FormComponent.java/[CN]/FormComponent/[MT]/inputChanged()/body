{
  if (isVisibleInHierarchy() && isEnabled()) {
    final String[] input=inputAsStringArray();
    if (input != null) {
      rawInput=StringList.valueOf(input).join(VALUE_SEPARATOR);
    }
 else     if (isInputNullable()) {
      rawInput=null;
    }
 else {
      rawInput=NO_RAW_INPUT;
    }
  }
}
