{
  if (type == null) {
    convertedInput=getInput();
  }
 else   if (!Strings.isEmpty(getInput())) {
    final IConverter converter=getConverter();
    try {
      convertedInput=converter.convert(getInput(),type);
    }
 catch (    Exception e) {
      error(Collections.singleton("TypeValidator"),new HashMap());
    }
  }
}
