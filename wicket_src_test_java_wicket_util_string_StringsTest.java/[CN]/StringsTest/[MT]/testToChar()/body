{
  assertEquals(' ',Strings.toChar(" "));
  assertEquals('a',Strings.toChar("a"));
  try {
    Strings.toChar("");
    fail("Exception expected");
  }
 catch (  StringValueConversionException e) {
  }
  try {
    Strings.toChar(null);
    fail("Exception expected");
  }
 catch (  StringValueConversionException e) {
  }
  try {
    Strings.toChar("aa");
    fail("Exception expected");
  }
 catch (  StringValueConversionException e) {
  }
}
