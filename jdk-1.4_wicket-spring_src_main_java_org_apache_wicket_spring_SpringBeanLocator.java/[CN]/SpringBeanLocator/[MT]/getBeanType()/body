{
  Class clazz=beanTypeCache == null ? null : (Class)beanTypeCache.get();
  if (clazz == null) {
    try {
      clazz=Class.forName(beanTypeName,true,Thread.currentThread().getContextClassLoader());
      beanTypeCache=new WeakReference(clazz);
    }
 catch (    ClassNotFoundException e) {
      throw new RuntimeException("SpringBeanLocator could not find class [" + beanTypeName + "] needed to locate the ["+ ((beanName != null) ? (beanName) : ("bean name not specified"))+ "] bean",e);
    }
  }
  return clazz;
}
