{
  setOutputMarkupId(true);
  add(new AjaxLink<Void>("updatePage"){
    @Override public void onClick(    AjaxRequestTarget target){
      target.add(getPage());
    }
  }
);
  add(new AjaxLink<Void>("updateComponent"){
    @Override public void onClick(    AjaxRequestTarget target){
      target.add(this);
    }
  }
.setOutputMarkupId(true));
}
