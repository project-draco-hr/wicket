{
  final Serializable key=resource.getCacheKey();
  String version=cache.get(key);
  if (version == null) {
    version=delegate.getVersion(resource);
    if (version == null) {
      version=NULL_VALUE;
    }
    cache.put(key,version);
  }
  if (version == NULL_VALUE) {
    return null;
  }
  return version;
}
