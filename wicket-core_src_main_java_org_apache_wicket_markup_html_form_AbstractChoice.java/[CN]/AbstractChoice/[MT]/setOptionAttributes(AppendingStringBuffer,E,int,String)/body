{
  if (isSelected(choice,index,selected)) {
    buffer.append("selected=\"selected\" ");
  }
  if (isDisabled(choice,index,selected)) {
    buffer.append("disabled=\"disabled\" ");
  }
  buffer.append("value=\"");
  buffer.append(Strings.escapeMarkup(renderer.getIdValue(choice,index)));
  buffer.append("\"");
}
