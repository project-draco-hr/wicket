{
  return new WicketFilter(){
    @Override public FilterConfig getFilterConfig(){
      return new FilterConfig(){
        @Override public String getInitParameter(        final String param){
          if ("module".equals(param)) {
            return TestModule.class.getName();
          }
          return null;
        }
        @Override public ServletContext getServletContext(){
          return new MockServletContext(null,null);
        }
        @Override public Enumeration<?> getInitParameterNames(){
          return null;
        }
        @Override public String getFilterName(){
          return null;
        }
      }
;
    }
  }
;
}
