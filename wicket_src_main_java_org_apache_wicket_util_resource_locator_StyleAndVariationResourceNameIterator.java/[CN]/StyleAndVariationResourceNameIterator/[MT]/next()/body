{
  if (state == 0) {
    state++;
    if ((style != null) && (variation != null)) {
      return path + '_' + variation+ '_'+ style;
    }
  }
  if (state == 1) {
    state++;
    if (style != null) {
      return path + '_' + style;
    }
  }
  if (state == 2) {
    state++;
    if (variation != null) {
      return path + '_' + variation;
    }
  }
  state=4;
  return path;
}
