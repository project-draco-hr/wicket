{
  if (!tag.isOpen())   return false;
  if (!"span".equalsIgnoreCase(tag.getName()) || tag.getNamespace() != null)   return false;
  String classNames=tag.getAttributes().getString("class");
  if (Strings.isEmpty(classNames))   return false;
  boolean textClassFound=false;
  for (  String className : classNames.split(" ")) {
    if ("label-text".equals(className)) {
      textClassFound=true;
      break;
    }
  }
  if (!textClassFound)   return false;
  return true;
}
