{
  RequestCycle requestCycle=RequestCycle.get();
  if (requestCycle != null) {
    SerializedPagesMetadataEntry entry=(SerializedPagesMetadataEntry)requestCycle.getMetaData(SERIALIZED_PAGES_KEY);
    if (entry == null) {
      entry=new SerializedPagesMetadataEntry();
      entry.sessionId=sessionId;
      requestCycle.setMetaData(SERIALIZED_PAGES_KEY,entry);
    }
    entry.map.put(page,pagesList);
  }
}
