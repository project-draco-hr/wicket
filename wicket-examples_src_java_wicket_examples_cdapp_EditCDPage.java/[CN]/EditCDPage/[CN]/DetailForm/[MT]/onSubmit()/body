{
  CD cd=(CD)getModelObject();
  Session session=null;
  Transaction tx=null;
  try {
    session=HibernateHelper.getSession();
    tx=session.beginTransaction();
    session.save(cd);
    tx.commit();
    info("cd saved");
  }
 catch (  HibernateException e) {
    try {
      tx.rollback();
    }
 catch (    HibernateException ex) {
      ex.printStackTrace();
    }
    throw new WicketRuntimeException(e);
  }
}
