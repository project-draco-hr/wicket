{
  if (!(o instanceof Entry)) {
    return null;
  }
  Entry<V> entry=(Entry<V>)o;
  int key=entry.getKey();
  int i=indexFor(key,table.length);
  Entry<V> prev=table[i];
  Entry<V> e=prev;
  while (e != null) {
    Entry<V> next=e.next;
    if ((e.key == key) && e.equals(entry)) {
      modCount++;
      size--;
      if (prev == e) {
        table[i]=next;
      }
 else {
        prev.next=next;
      }
      return e;
    }
    prev=e;
    e=next;
  }
  return e;
}
