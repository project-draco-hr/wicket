{
  return new Iterator<Item<T>>(){
    private int index=0;
    public void remove(){
      throw new UnsupportedOperationException();
    }
    public boolean hasNext(){
      return newModels.hasNext();
    }
    public Item<T> next(){
      final IModel<T> model=newModels.next();
      Item<T> item=factory.newItem(parent,index,model);
      index++;
      return item;
    }
  }
;
}
