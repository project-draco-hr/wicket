{
  if (aborted)   throw new AssertionError("Request was checked and aborted");
  if (suppressed)   throw new AssertionError("Request was checked and suppressed");
  if (allowed)   throw new AssertionError("Request was checked and allowed");
  if (whitelisted)   throw new AssertionError("Origin was whitelisted");
  if (matched)   throw new AssertionError("Origin was checked and matched");
}
