{
  DefaultButtonImageResource image=(DefaultButtonImageResource)letterToImage.get(letter);
  if (image == null) {
    image=new DefaultButtonImageResource(30,30,letter.asString());
    if (letter.isGuessed()) {
      image.setColor(Color.GRAY);
    }
    letterToImage.put(letter,image);
  }
  return image;
}
