{
  final StringBuffer buffer=new StringBuffer();
  buffer.append("shared/");
  buffer.append(scope.getName());
  buffer.append('_');
  buffer.append(name);
  if (locale != null) {
    buffer.append('_');
    buffer.append(locale.toString());
  }
  if (style != null) {
    buffer.append('_');
    buffer.append(style);
  }
  return buffer.toString();
}
