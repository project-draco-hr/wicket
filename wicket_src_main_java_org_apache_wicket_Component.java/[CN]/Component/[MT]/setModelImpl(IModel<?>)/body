{
  if (getFlag(FLAG_MODEL_SET)) {
    if (model != null) {
      data_set(0,model);
    }
 else {
      if (data_can_remove(0)) {
        data_remove(0);
        setFlag(FLAG_MODEL_SET,false);
      }
 else {
        data_set(0,null);
      }
    }
  }
 else {
    if (model != null) {
      data_insert(0,model);
      setFlag(FLAG_MODEL_SET,true);
    }
  }
}
