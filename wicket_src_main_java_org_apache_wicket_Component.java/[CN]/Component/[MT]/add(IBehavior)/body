{
  if (behaviors == null) {
    throw new IllegalArgumentException("Argument may not be null");
  }
  for (  IBehavior behavior : behaviors) {
    if (behavior == null) {
      throw new IllegalArgumentException("Argument may not be null");
    }
    addBehavior(behavior);
    if (!behavior.isTemporary()) {
      addStateChange();
    }
    behavior.bind(this);
  }
  return this;
}
