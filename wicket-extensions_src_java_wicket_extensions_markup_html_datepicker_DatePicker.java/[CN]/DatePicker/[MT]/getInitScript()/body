{
  StringBuffer b=new StringBuffer("\nCalendar.setup(\n{");
  b.append("\n\t\tinputField : \"").append(target.getPath()).append("\",");
  b.append("\n\t\tbutton : \"").append(triggerButton.getPath()).append("\",");
