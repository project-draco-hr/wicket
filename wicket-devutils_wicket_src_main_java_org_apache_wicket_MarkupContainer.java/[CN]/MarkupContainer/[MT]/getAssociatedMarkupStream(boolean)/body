{
  try {
    return getApplication().getMarkupSettings().getMarkupCache().getMarkupStream(this,false,throwException);
  }
 catch (  MarkupException ex) {
    throw ex;
  }
catch (  MarkupNotFoundException ex) {
    throw ex;
  }
catch (  WicketRuntimeException ex) {
    throw new MarkupNotFoundException(exceptionMessage("Markup of type '" + getMarkupType() + "' for component '"+ getClass().getName()+ "' not found."+ " Enable debug messages for org.apache.wicket.util.resource to get a list of all filenames tried"),ex);
  }
}
