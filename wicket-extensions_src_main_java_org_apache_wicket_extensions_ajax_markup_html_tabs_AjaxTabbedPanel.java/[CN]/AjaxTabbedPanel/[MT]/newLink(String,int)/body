{
  return new AjaxFallbackLink<Void>(linkId){
    private static final long serialVersionUID=1L;
    @Override public void onClick(    final AjaxRequestTarget target){
      setSelectedTab(index);
      if (target != null) {
        target.add(AjaxTabbedPanel.this);
      }
      onAjaxUpdate(target);
    }
  }
;
}
