{
  final AppendingStringBuffer buf=new AppendingStringBuffer(400);
  buf.append(this.markup.toString());
  buf.append("\n");
  final Iterator<MarkupElement> iter=this.markupElements.iterator();
  while (iter.hasNext()) {
    buf.append(iter.next());
    buf.append(",");
  }
  return buf.toString();
}
