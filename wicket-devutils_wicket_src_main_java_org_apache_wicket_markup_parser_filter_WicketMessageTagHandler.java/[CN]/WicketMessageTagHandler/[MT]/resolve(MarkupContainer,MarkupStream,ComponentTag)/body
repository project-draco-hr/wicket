{
  if (WICKET_MESSAGE_CONTAINER_ID.equals(tag.getId())) {
    Component wc=null;
    if (tag.isOpenClose()) {
      wc=new WebComponent(WICKET_MESSAGE_CONTAINER_ID + container.getPage().getAutoIndex());
    }
 else {
      wc=new TransparentContainer(WICKET_MESSAGE_CONTAINER_ID + container.getPage().getAutoIndex());
    }
    container.autoAdd(wc,markupStream);
    return true;
  }
  return false;
}
