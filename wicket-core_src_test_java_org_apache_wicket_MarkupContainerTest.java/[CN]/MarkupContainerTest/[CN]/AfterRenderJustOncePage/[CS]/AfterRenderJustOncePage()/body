{
  WebMarkupContainer a1=new WebMarkupContainer("a1");
  add(a1);
  WebMarkupContainer a2=new WebMarkupContainer("a2");
  a1.add(a2);
  WebMarkupContainer a3=new WebMarkupContainer("a3"){
    @Override protected void onAfterRender(){
      super.onAfterRender();
      afterRenderCalls++;
    }
  }
;
  a2.add(a3);
}
