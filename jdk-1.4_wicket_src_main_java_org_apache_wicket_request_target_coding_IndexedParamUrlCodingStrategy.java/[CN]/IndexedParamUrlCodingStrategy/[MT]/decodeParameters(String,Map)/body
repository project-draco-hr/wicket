{
  PageParameters params=new PageParameters();
  if (urlFragment == null) {
    return params;
  }
  if (urlFragment.startsWith("/")) {
    urlFragment=urlFragment.substring(1);
  }
  String[] parts=urlFragment.split("/");
  for (int i=0; i < parts.length; i++) {
    if (WebRequestCodingStrategy.PAGEMAP.equals(parts[i])) {
      i++;
      params.put(WebRequestCodingStrategy.PAGEMAP,WebRequestCodingStrategy.decodePageMapName(urlDecode(parts[i])));
    }
 else {
      params.put(String.valueOf(i),urlDecode(parts[i]));
    }
  }
  return params;
}
