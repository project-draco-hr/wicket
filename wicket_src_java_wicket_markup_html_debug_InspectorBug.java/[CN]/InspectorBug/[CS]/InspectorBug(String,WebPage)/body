{
  super(id);
  Link link;
  if (page.isStateless()) {
    link=new BookmarkablePageLink("link",InspectorPage.class);
  }
 else {
    link=new Link("link"){
      private static final long serialVersionUID=1L;
      public void onClick(){
        setResponsePage(new InspectorPage(page));
      }
    }
;
  }
  link.add(new Image("bug"));
  add(link);
}
