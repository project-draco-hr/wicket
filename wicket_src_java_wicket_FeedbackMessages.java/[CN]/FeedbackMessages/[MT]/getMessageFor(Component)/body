{
  if (messages != null) {
    FeedbackMessage message=null;
    for (Iterator i=messages.iterator(); i.hasNext(); ) {
      FeedbackMessage toTest=(FeedbackMessage)i.next();
      if ((toTest.getReporter() != null) && (toTest.getReporter().equals(component))) {
        message=toTest;
        break;
      }
    }
    return message;
  }
 else {
    return null;
  }
}
