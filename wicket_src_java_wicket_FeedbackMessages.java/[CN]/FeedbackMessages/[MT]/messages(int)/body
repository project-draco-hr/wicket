{
  List list=null;
  if (messages != null) {
    for (final Iterator i=messages.iterator(); i.hasNext(); ) {
      final FeedbackMessage message=(FeedbackMessage)i.next();
      if (message.isLevel(level)) {
        if (list == null) {
          list=new ArrayList();
        }
        list.add(message);
      }
    }
  }
  return list != null ? Collections.unmodifiableList(list) : Collections.EMPTY_LIST;
}
