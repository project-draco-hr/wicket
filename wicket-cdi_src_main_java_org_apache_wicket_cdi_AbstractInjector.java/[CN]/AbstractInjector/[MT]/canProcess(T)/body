{
  final boolean canProcess;
  Class<?> instanceClass=instance.getClass();
  if (instanceClass.isAnonymousClass() || (instanceClass.isMemberClass() && Modifier.isStatic(instanceClass.getModifiers()) == false)) {
    canProcess=false;
    LOG.debug("Class '{}' will not be processed for CDI injection because it is anonymous or non-static member class",instanceClass);
  }
 else {
    canProcess=true;
    LOG.debug("Going to process class '{}' for CDI injection",instanceClass);
  }
  return canProcess;
}
