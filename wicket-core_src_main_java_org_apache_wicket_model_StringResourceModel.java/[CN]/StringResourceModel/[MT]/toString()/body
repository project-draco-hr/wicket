{
  StringBuilder sb=new StringBuilder("StringResourceModel[");
  sb.append("key:");
  sb.append(resourceKey);
  sb.append(",default:");
  sb.append(defaultValue);
  sb.append(",params:");
  if (parameters != null) {
    sb.append(Arrays.asList(parameters));
  }
  sb.append("]");
  return sb.toString();
}
