{
  this.current=(Panel)panel1.reattach();
  add(new PageLink(this,"link",new IPageLink(){
    private static final long serialVersionUID=1L;
    public Page getPage(){
      replacePanel();
      return TestPage.this;
    }
    public Class getPageIdentity(){
      return TestPage.this.getClass();
    }
  }
));
}
