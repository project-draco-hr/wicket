{
  if (parameters != null) {
    Iterator entries=parameters.entrySet().iterator();
    while (entries.hasNext()) {
      Map.Entry entry=(Entry)entries.next();
      String escapedValue=(String)entry.getValue();
      try {
        escapedValue=URLEncoder.encode(escapedValue,Application.get().getRequestCycleSettings().getResponseRequestEncoding());
      }
 catch (      UnsupportedEncodingException e) {
        log.error(e.getMessage(),e);
      }
      url.append("/").append(entry.getKey()).append("/").append(escapedValue);
    }
  }
}
