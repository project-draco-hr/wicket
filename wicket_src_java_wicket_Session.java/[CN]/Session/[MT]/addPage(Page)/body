{
  page.setId(this.state.pageId++);
  state.dirty=true;
  final MostRecentlyUsedMap pageMap=getPageMap();
  pageMap.put(new Integer(page.getId()),page);
  final Page removedPage=(Page)pageMap.getRemovedValue();
  if (removedPage != null) {
    removeAttribute(removedPage.getName());
  }
  pageChanged(page);
}
