{
  Page page=getPage(Integer.parseInt(Strings.firstPathComponent(path,componentPathSeparator)));
  if (page != null) {
    final Page version=page.getVersion(versionNumber);
    if (version != null) {
      if (version != page) {
        page=version;
        getPageMap().put(new Integer(page.getId()),page);
        pageChanged(page);
      }
      page.onInternalBeginRequest();
      page.onBeginRequest();
      return page;
    }
  }
  return null;
}
