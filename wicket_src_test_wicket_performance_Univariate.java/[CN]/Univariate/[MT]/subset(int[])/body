{
  final int k=indices.length;
  int i=0;
  final double elements[]=new double[k];
  try {
    for (i=0; i < k; i++) {
      elements[i]=x[k];
    }
  }
 catch (  final ArrayIndexOutOfBoundsException e) {
    System.out.println("Index " + i + " does not exist in data.");
  }
  return (elements);
}
