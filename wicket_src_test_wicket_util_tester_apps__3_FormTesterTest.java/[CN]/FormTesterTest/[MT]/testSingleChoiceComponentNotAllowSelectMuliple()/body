{
  try {
    formTester.selectMultiple("dropDownChoice",new int[]{0});
    throw new RuntimeException("AssertionFailedError expected");
  }
 catch (  AssertionFailedError expected) {
  }
  try {
    formTester.selectMultiple("radioGroup",new int[]{2,1});
    throw new RuntimeException("AssertionFailedError expected");
  }
 catch (  AssertionFailedError expected) {
  }
}
