{
  if ((key != null) && (key.length() > 0)) {
    Map mapper;
    String charset;
    for (int i=0; i < mappers.length; i++) {
      mapper=mappers[i];
      if (mapper != null) {
        charset=(String)mapper.get(key);
        if (charset != null) {
          if (i > MAP_CACHE) {
            mappers[MAP_CACHE].put(key,charset);
          }
          return charset;
        }
      }
    }
    mappers[MAP_CACHE].put(key,"");
  }
  return "";
}
