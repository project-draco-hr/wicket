{
  try {
    final String url=getDocumentBase() + getParameter("getModelUrl");
    InputStream in=new URL(url).openStream();
    return new ObjectInputStream(in).readObject();
  }
 catch (  ClassNotFoundException e) {
    e.printStackTrace();
    throw new RuntimeException(e);
  }
catch (  IOException e) {
    e.printStackTrace();
    throw new RuntimeException(e);
  }
}
