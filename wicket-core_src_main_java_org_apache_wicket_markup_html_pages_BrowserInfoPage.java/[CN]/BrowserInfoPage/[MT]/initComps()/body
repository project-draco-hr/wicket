{
  WebComponent meta=new WebComponent("meta");
  PageParameters parameters=new PageParameters();
  parameters.set("cto",continueTo);
  CharSequence url=urlFor(new BookmarkablePageRequestHandler(new PageProvider(BrowserInfoPage.class,parameters)));
  meta.add(AttributeModifier.replace("content",Model.of("0; url=" + url)));
  add(meta);
  WebMarkupContainer link=new WebMarkupContainer("link");
  link.add(AttributeModifier.replace("href",Model.of((Serializable)url)));
  add(link);
  add(new BrowserInfoForm("postback"){
    private static final long serialVersionUID=1L;
    /** 
 * @see org.apache.wicket.markup.html.pages.BrowserInfoForm#afterSubmit()
 */
    @Override protected void afterSubmit(){
      continueToPrevious();
    }
  }
);
}
