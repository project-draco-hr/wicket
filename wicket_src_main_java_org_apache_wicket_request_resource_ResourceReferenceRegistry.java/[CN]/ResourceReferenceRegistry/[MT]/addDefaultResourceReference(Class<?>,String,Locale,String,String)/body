{
  ResourceReference reference=createDefaultResourceReference(scope,name,locale,style,variation);
  if (reference != null) {
    enforceAutoAddedCacheSize(getAutoAddedCapacity());
    Key key=_registerResourceReference(reference);
    if (autoAddedQueue != null) {
      autoAddedQueue.add(key);
    }
  }
 else {
    log.warn("Asked to auto-create a ResourceReference, but ResourceReferenceRegistry.createDefaultResourceReference() return null. " + " [scope: " + scope.getName() + "; name: "+ name+ "; locale: "+ locale+ "; style: "+ style+ "; variation: "+ variation+ "]");
  }
  return reference;
}
