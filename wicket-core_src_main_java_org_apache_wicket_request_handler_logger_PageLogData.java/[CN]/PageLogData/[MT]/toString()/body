{
  StringBuilder sb=new StringBuilder("{");
  if (pageClass != null) {
    sb.append("pageClass=");
    sb.append(getPageClass().getName());
    sb.append(',');
  }
  sb.append("pageId=");
  sb.append(getPageId());
  sb.append(",pageParameters={");
  sb.append(getPageParameters());
  sb.append("},renderCount=");
  sb.append(getRenderCount());
  sb.append("}");
  return sb.toString();
}
