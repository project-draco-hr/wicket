{
  assertNull(Strings.escapeMarkup(null,true,true));
  assertEquals("",Strings.escapeMarkup("",true,true).toString());
  assertEquals("&#199;&#252;&#233;&#226;&#228;&#224;&#229;&#231;&#234;&#235;",Strings.escapeMarkup("\u00c7\u00fc\u00e9\u00e2\u00e4\u00e0\u00e5\u00e7\u00ea\u00eb",false,true).toString());
}
