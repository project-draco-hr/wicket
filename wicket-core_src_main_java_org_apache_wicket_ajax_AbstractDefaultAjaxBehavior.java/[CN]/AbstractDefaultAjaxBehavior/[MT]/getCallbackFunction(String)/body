{
  StringBuilder sb=new StringBuilder();
  sb.append("function (");
  boolean first=true;
  for (  String curExtraParameter : extraParameters) {
    if (!first)     sb.append(",");
 else     first=false;
    sb.append(curExtraParameter);
  }
  sb.append(") {\n");
  sb.append(getCallbackFunctionBody(extraParameters));
  sb.append("}\n");
  return sb;
}
