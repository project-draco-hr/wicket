{
  Args.notEmpty(script,"script");
  Args.notEmpty(throttleId,"throttleId");
  Args.notNull(throttleDelay,"throttleDelay");
  return new AppendingStringBuffer("wicketThrottler.throttle( '").append(throttleId).append("', ").append(throttleDelay.getMilliseconds()).append(", function() { ").append(script).append("}.bind(this));");
}
