{
  Args.notEmpty(script,"script");
  Args.notEmpty(throttleId,"throttleId");
  Args.notNull(throttleDelay,"throttleDelay");
  return new AppendingStringBuffer("Wicket.throttler.throttle( '").append(throttleId).append("', ").append(throttleDelay.getMilliseconds()).append(", Wicket.inCtx(function() { ").append(script).append("}, this));");
}
