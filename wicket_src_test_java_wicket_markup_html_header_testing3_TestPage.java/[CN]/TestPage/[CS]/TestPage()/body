{
  this.current=panel1;
  add(current);
  add(new PageLink("link",new IPageLink(){
    private static final long serialVersionUID=1L;
    public Page getPage(){
      replacePanel();
      return TestPage.this;
    }
    public Class getPageIdentity(){
      return TestPage.this.getClass();
    }
  }
));
}
