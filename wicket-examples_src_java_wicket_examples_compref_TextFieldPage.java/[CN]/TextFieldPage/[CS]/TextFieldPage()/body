{
  final TextFieldPageInput textFieldPageInput=new TextFieldPageInput();
  setModel(new CompoundPropertyModel<TextFieldPageInput>(textFieldPageInput));
  FeedbackPanel feedbackPanel=new FeedbackPanel(this,"feedback");
