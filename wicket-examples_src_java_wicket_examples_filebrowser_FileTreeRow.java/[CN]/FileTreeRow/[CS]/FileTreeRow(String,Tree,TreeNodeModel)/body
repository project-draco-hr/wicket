{
  super(componentName);
  if (nodeModel != null) {
    TreeNodeLink link=new TreeNodeLink("link",tree,nodeModel);
    File file=(File)nodeModel.getUserObject();
    link.add(new Label("label",file.getName()));
    add(link);
  }
 else {
    add(new HtmlContainer("link").add(new HtmlContainer("label")));
  }
}
