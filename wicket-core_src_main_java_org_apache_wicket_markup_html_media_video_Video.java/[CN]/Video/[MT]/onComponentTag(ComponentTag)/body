{
  this.checkComponentTag(tag,"video");
  super.onComponentTag(tag);
  if (this.width != null) {
    tag.put("width",this.width);
  }
  if (this.height != null) {
    tag.put("height",this.height);
  }
  if (this.poster != null) {
    tag.put("poster",RequestCycle.get().urlFor(this.poster,this.posterPageParameters));
  }
}
