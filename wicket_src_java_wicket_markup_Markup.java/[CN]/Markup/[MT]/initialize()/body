{
  if (markup != null) {
    for (int i=0; i < markup.size(); i++) {
      MarkupElement elem=(MarkupElement)markup.get(i);
      if (elem instanceof WicketTag) {
        WicketTag tag=(WicketTag)elem;
        if ((tag.isHeadTag() == true) && (tag.getNamespace() != null)) {
          headerIndex=i;
          break;
        }
      }
    }
  }
}
