{
  final IPageMapEntry entry=page.getPageMapEntry();
  removeEntry(entry);
  final Iterator stack=accessStack.iterator();
  while (stack.hasNext()) {
    final Access access=(Access)stack.next();
    if (access.id == entry.getNumericId()) {
      stack.remove();
    }
  }
  session.dirtyPageMap(this);
}
