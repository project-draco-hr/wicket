{
  List temp=new ArrayList();
  for (int i=0; i < drops.length; i++) {
    if (inclAlterFlag) {
      if (drops[i].toLowerCase().trim().startsWith("alter")) {
        temp.add(drops[i]);
      }
    }
 else {
      if (!drops[i].toLowerCase().trim().startsWith("alter")) {
        temp.add(drops[i]);
      }
    }
  }
  return (String[])temp.toArray(new String[temp.size()]);
}
