{
  Application application=Application.get();
  addJsReference(container,new PackageResourceReference(application,ScriptaculousAjaxHandler.class,"prototype.js"));
  addJsReference(container,new PackageResourceReference(application,ScriptaculousAjaxHandler.class,"controls.js"));
  addJsReference(container,new PackageResourceReference(application,ScriptaculousAjaxHandler.class,"dragdrop.js"));
  addJsReference(container,new PackageResourceReference(application,ScriptaculousAjaxHandler.class,"effects.js"));
  addJsReference(container,new PackageResourceReference(application,ScriptaculousAjaxHandler.class,"scriptaculous.js"));
}
