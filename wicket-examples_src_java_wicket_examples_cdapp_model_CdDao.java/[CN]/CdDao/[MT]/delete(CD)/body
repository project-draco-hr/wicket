{
  if (cd == null) {
    throw new NullPointerException("cd must be not null");
  }
  Transaction tx=null;
  try {
    tx=hibernateSession.beginTransaction();
    hibernateSession.delete(cd);
    tx.commit();
  }
 catch (  HibernateException e) {
    try {
      tx.rollback();
    }
 catch (    HibernateException ex) {
      ex.printStackTrace();
    }
    throw new WicketRuntimeException(e);
  }
}
