{
  LoadableDetachableModel model=new MyLoadableDetachableModel();
  String expected="Model:classname=[" + model.getClass().getName() + "]"+ ":attached=false"+ ":tempModelObject=[null]";
  assertEquals(expected,model.toString());
  model.getObject();
  expected="Model:classname=[" + model.getClass().getName() + "]"+ ":attached=true"+ ":tempModelObject=[foo]";
  assertEquals(expected,model.toString());
  model.detach();
  expected="Model:classname=[" + model.getClass().getName() + "]"+ ":attached=false"+ ":tempModelObject=[null]";
  assertEquals(expected,model.toString());
}
