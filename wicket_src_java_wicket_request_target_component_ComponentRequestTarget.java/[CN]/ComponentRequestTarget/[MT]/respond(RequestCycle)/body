{
  Page page=component.getPage();
  if (page != null) {
    page.startComponentRender(component);
  }
  if (component instanceof Page) {
    new PageRequestTarget((Page)component).respond(requestCycle);
  }
 else {
    component.doRender();
  }
  if (page != null) {
    page.endComponentRender(component);
  }
}
