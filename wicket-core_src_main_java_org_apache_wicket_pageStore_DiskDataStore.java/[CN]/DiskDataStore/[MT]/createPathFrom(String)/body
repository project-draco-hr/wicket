{
  int hash=Math.abs(sessionId.hashCode());
  String low=String.valueOf(hash % 9973);
  String high=String.valueOf((hash / 9973) % 9973);
  StringBuilder bs=new StringBuilder(sessionId.length() + 10);
  bs.append(low);
  bs.append(File.separator);
  bs.append(high);
  bs.append(File.separator);
  bs.append(sessionId);
  return bs.toString();
}
