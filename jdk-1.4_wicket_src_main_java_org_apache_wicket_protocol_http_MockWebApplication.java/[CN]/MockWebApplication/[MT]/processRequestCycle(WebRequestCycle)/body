{
  try {
    cycle.request();
    if (cycle.wasHandled() == false) {
      cycle.setRequestTarget(new WebErrorCodeResponseTarget(HttpServletResponse.SC_NOT_FOUND));
    }
    cycle.detach();
    createRequestCycle();
  }
  finally {
    cycle.getResponse().close();
  }
  postProcessRequestCycle(cycle);
}
