{
  super(id);
  if (!(list.getModelObject() instanceof ISortableDataProvider)) {
    throw new IllegalStateException("An OrderByLink must point to a " + "DataView that uses an ISortableDataProvider as it's model.");
  }
  this.sortProperty=sortProperty;
  this.list=list;
  add(new CssModifier(this,cssProvider));
}
