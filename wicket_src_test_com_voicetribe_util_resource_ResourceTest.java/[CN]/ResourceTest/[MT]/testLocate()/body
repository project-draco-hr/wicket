{
  executeMultiple(null);
  Resource resource=Resource.locate(null,this.getClass().getClassLoader(),this.getClass().getName(),null,null,"txt");
  String path=Strings.replaceAll(resource.getFile().getAbsolutePath(),"\\","/");
  path=Strings.beforeLastPathComponent(path,'/') + "/sourcePath";
  executeMultiple(new Path(new Folder(path)));
}
