{
  if (tag.isWicketBodyTag()) {
    if (this.body == null) {
      throw new MarkupException(markupStream,"Border body container not initialized. Did you forget to call setBorderBodyContainer() ??");
    }
    if (markupStream.get() == this.body.getMarkupFragment().get(0)) {
      this.body.render(markupStream);
      return true;
    }
  }
  return false;
}
