{
  pojo=new Pojo("Mock name");
  Form form=new Form(this,"form",new CompoundPropertyModel(pojo));
  new TextField(form,"name");
  WebComponent eventComponent=new WebComponent(this,EVENT_COMPONENT);
  eventComponent.add(new AjaxFormSubmitBehavior(form,ClientEvent.CLICK){
    private static final long serialVersionUID=1L;
    @Override protected void onSubmit(    AjaxRequestTarget target){
      executed=true;
    }
  }
);
}
