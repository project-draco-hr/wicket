{
  this.openTag=openTag;
  this.haveSeenBodyTag=false;
  renderAssociatedMarkup("border","Markup for a border component must begin a tag like '<org.apache.wicket:border>'");
  if (haveSeenBodyTag == false) {
    markupStream.throwMarkupException("Didn't find <org.apache.wicket:body/> tag for the border compoment.");
  }
}
