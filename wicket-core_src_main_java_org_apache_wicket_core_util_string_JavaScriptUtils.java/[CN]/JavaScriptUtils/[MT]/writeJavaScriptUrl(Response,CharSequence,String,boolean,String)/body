{
  response.write("<script type=\"text/javascript\" ");
  if (id != null) {
    response.write("id=\"" + id + "\" ");
  }
  if (defer) {
    response.write("defer=\"defer\" ");
  }
  if (charset != null) {
    response.write("charset=\"" + charset + "\" ");
  }
  response.write("src=\"");
  response.write(url);
  response.write("\"></script>");
  response.write("\n");
}
