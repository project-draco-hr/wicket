{
  response.write("<script type=\"text/javascript\" ");
  if (id != null) {
    response.write("id=\"" + Strings.escapeMarkup(id) + "\" ");
  }
  if (defer) {
    response.write("defer=\"defer\" ");
  }
  if (charset != null) {
    response.write("charset=\"" + Strings.escapeMarkup(charset) + "\" ");
  }
  response.write("src=\"");
  response.write(url);
  response.write("\"></script>");
  response.write("\n");
}
