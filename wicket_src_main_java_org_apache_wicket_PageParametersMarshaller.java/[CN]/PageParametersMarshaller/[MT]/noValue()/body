{
switch (this) {
case BOOLEAN:
    return Boolean.FALSE;
case INT:
  return -1;
case STRING:
return null;
case LONG:
return -1L;
case DOUBLE:
return -1D;
case FLOAT:
return -1F;
case BYTE:
return new Byte((byte)-1);
case SHORT:
return new Short((short)-1);
case CHAR:
return (char)0;
default :
return null;
}
}
