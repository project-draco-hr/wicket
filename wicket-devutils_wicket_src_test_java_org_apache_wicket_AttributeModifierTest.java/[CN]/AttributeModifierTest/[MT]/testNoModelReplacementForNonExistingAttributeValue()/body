{
  AttributeModifier modifier=new AttributeModifier("test",false,new Model<String>("Ellioth Smith Rocks"));
  XmlTag xmlTag=new XmlTag();
  ComponentTag tag=new ComponentTag(xmlTag);
  tag.setId("test");
  tag.setName("id");
  modifier.replaceAttributeValue(null,tag);
  Map<String,Object> attributes=tag.getAttributes();
  assertTrue("attribute should not be added, as it didn't exist yet",attributes.isEmpty());
}
