{
  if (pageClass != null) {
    if (requestCycle.getRedirect()) {
      IRequestCycleProcessor processor=requestCycle.getProcessor();
      String redirectUrl=processor.getRequestCodingStrategy().encode(requestCycle,new BookmarkablePageRequestTarget(pageClass,pageParameters));
      requestCycle.getResponse().redirect(redirectUrl);
    }
 else {
      requestCycle.setUpdateSession(true);
      page=getPage(requestCycle);
      page.doRender();
    }
  }
}
