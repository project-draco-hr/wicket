{
  return new PanelMarkupSourcingStrategy(false){
    @Override public IMarkupFragment getMarkup(    final MarkupContainer parent,    final Component child){
      MarkupStream markup=new MarkupStream(parent.getAssociatedMarkup());
      markup.skipRawMarkup();
      return markup.getMarkupFragment();
    }
    @Override public void onComponentTagBody(    Component component,    MarkupStream markupStream,    ComponentTag openTag){
      HeaderSectionMyLabel.this.onComponentTagBody(markupStream,openTag);
    }
  }
;
}
