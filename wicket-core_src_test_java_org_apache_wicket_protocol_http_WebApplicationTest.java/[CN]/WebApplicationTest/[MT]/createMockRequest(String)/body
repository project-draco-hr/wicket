{
  final Url url=Url.parse(path);
  return new Request(){
    @Override public Url getUrl(){
      return url;
    }
    @Override public Url getClientUrl(){
      return url;
    }
    @Override public Locale getLocale(){
      return null;
    }
    @Override public Charset getCharset(){
      return null;
    }
    @Override public Object getContainerRequest(){
      return null;
    }
  }
;
}
