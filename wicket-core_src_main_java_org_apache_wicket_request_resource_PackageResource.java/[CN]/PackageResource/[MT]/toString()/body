{
  final StringBuilder result=new StringBuilder();
  result.append('[').append(getClass().getSimpleName()).append(' ').append("name = ").append(path).append(", scope = ").append(scopeName).append(", locale = ").append(locale).append(", style = ").append(style).append(", variation = ").append(variation).append(']');
  return result.toString();
}
