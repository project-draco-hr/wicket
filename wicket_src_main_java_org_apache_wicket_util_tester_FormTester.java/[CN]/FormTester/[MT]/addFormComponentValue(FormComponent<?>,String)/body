{
  if (parameterExist(formComponent)) {
    List<StringValue> values=tester.getRequest().getPostRequestParameters().getParameterValues(formComponent.getInputName());
    HashSet<String> all=new HashSet<String>();
    for (    StringValue val : values) {
      all.add(val.toString());
    }
    all.add(value);
    values=new ArrayList<StringValue>();
    for (    String val : all) {
      values.add(StringValue.valueOf(val));
    }
    tester.getRequest().getPostRequestParameters().setParameterValues(formComponent.getInputName(),values);
  }
 else {
    setFormComponentValue(formComponent,value);
  }
}
