{
  IModel<Object> dataModel=new IModel<Object>(){
    @Override public Object getObject(){
      T before=rowModel.getObject();
      if (before == null) {
        return null;
      }
 else {
        return function.apply(before);
      }
    }
    @Override public void detach(){
      rowModel.detach();
    }
  }
;
  return dataModel;
}
