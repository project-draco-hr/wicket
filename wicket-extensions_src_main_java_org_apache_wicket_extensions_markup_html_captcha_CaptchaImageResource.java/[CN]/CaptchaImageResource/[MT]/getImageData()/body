{
  byte[] data=null;
  if (imageData != null && challengeIdhashCode != null && challengeIdhashCode.equals(challengeId.getObject().hashCode())) {
    data=imageData.get();
  }
  if (data == null) {
    data=render();
    imageData=new SoftReference<byte[]>(data);
    setLastModifiedTime(Time.now());
  }
  return data;
}
