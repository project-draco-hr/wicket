{
  IResourceStream markupResource=application.getResourceSettings().getResourceStreamLocator().locate(containerClass,containerClass.getName().replace('.','/'),containerInfo.getStyle(),containerInfo.getLocale(),containerInfo.getFileExtension());
  if (markupResource == null) {
    return null;
  }
  return new MarkupResourceStream(markupResource,containerInfo,containerClass);
}
