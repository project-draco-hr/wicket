{
  if (multiPart != 0) {
    return true;
  }
  Boolean anyEmbeddedMultipart=visitChildren(Component.class,new IVisitor<Component,Boolean>(){
    public void component(    final Component component,    final IVisit<Boolean> visit){
      boolean isMultiPart=false;
      if (component instanceof Form<?>) {
        Form<?> form=(Form<?>)component;
        isMultiPart=(form.multiPart != 0);
      }
 else       if (component instanceof FormComponent<?>) {
        FormComponent<?> form=(FormComponent<?>)component;
        isMultiPart=form.isMultiPart();
      }
      if (isMultiPart == true) {
        visit.stop(true);
      }
    }
  }
);
  return Boolean.TRUE.equals(anyEmbeddedMultipart);
}
