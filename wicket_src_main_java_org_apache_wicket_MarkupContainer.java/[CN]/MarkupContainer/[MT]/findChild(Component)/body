{
  if (get(child.getId()) != null) {
    return this;
  }
  MarkupContainer container=(MarkupContainer)visitChildren(MarkupContainer.class,new IVisitor<MarkupContainer>(){
    public Object component(    MarkupContainer container){
      if (container.isTransparentResolver()) {
        if (container.getId().equals(child.getId())) {
          return container;
        }
        return IVisitor.CONTINUE_TRAVERSAL;
      }
      return IVisitor.CONTINUE_TRAVERSAL_BUT_DONT_GO_DEEPER;
    }
  }
);
  if (container != null) {
    return container;
  }
  return null;
}
