{
  if (component == null) {
    throw new IllegalArgumentException("argument component may not be null");
  }
  component.setAuto(true);
  int index=children_indexOf(component);
  if (index >= 0) {
    children_remove(index);
  }
  add(component);
  component.render();
  if (markupStream != null) {
    markupStream.skipComponent();
  }
  return true;
}
