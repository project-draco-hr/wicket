{
  if (this == obj) {
    return true;
  }
  if (obj instanceof ResourceReference == false) {
    return false;
  }
  ResourceReference that=(ResourceReference)obj;
  return Objects.equal(scope,that.scope) && Objects.equal(name,that.name) && Objects.equal(locale,that.locale)&& Objects.equal(style,that.style)&& Objects.equal(variation,that.variation);
}
