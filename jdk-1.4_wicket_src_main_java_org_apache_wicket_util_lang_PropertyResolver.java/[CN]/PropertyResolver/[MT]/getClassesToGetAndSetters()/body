{
  Object key=null;
  if (Application.exists()) {
    key=Application.get();
  }
 else {
    key=PropertyResolver.class;
  }
  Map<Class,Map> result=applicationToClassesToGetAndSetters.get(key);
  if (result == null) {
    applicationToClassesToGetAndSetters.put(key,result=new ConcurrentHashMap<Class,Map>(64));
  }
  return result;
}
