{
  Object key=null;
  if (Application.exists()) {
    key=Application.get();
  }
 else {
    key=PropertyResolver.class;
  }
  Map result=(Map)applicationToClassesToGetAndSetters.get(key);
  if (result == null) {
    applicationToClassesToGetAndSetters.put(key,result=new ConcurrentHashMap(64));
  }
  return result;
}
