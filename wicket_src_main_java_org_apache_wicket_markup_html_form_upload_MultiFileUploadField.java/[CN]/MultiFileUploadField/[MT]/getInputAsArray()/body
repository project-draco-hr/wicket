{
  if (inputArrayCache == null) {
    ArrayList<String> names=null;
    final Request request=getRequest();
    if (request instanceof IMultipartWebRequest) {
      final Map itemNameToItem=((IMultipartWebRequest)request).getFiles();
      Iterator it=itemNameToItem.entrySet().iterator();
      while (it.hasNext()) {
        final Entry entry=(Entry)it.next();
        final String name=(String)entry.getKey();
        final FileItem item=(FileItem)entry.getValue();
        if (!Strings.isEmpty(name) && name.startsWith(getInputName() + MAGIC_SEPARATOR) && !Strings.isEmpty(item.getName())) {
          names=(names != null) ? names : new ArrayList<String>();
          names.add(name);
        }
      }
    }
    if (names != null) {
      inputArrayCache=names.toArray(new String[names.size()]);
    }
  }
  return inputArrayCache;
}
