{
  final String href=tag.getAttributes().getString("href");
  if ((tag.getId() == null) && (href != null) && (href.indexOf(":") == -1)) {
    if (href.endsWith(".html") || (href.indexOf(".html?") != -1)) {
      return true;
    }
    if ("link".equals(tag.getName())) {
      return true;
    }
  }
  return false;
}
