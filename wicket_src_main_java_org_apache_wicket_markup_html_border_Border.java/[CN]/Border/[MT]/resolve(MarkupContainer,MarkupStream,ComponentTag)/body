{
  if (rendering == false) {
    if (tag instanceof WicketTag) {
      WicketTag wtag=(WicketTag)tag;
      if (wtag.isBodyTag()) {
        rendering=true;
        try {
          body.render(markupStream);
        }
  finally {
          rendering=false;
        }
        return true;
      }
    }
  }
  return false;
}
