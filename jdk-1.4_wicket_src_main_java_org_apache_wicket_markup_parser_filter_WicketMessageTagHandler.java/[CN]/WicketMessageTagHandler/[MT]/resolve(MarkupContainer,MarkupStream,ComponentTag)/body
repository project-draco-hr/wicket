{
  if (WICKET_MESSAGE_CONTAINER_ID.equals(tag.getId())) {
    Component wc=null;
    if (tag.isOpenClose()) {
      wc=new WebComponent(WICKET_MESSAGE_CONTAINER_ID + container.getPage().getAutoIndex());
    }
 else {
      wc=new WebMarkupContainer(WICKET_MESSAGE_CONTAINER_ID + container.getPage().getAutoIndex());
    }
    wc.add(new AttributeLocalizer());
    container.autoAdd(wc);
    return true;
  }
  return false;
}
