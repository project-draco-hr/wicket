{
  checkComponentTag(tag,"select");
  super.onComponentTag(tag);
  IValueMap attrs=tag.getAttributes();
  attrs.put("multiple",null);
  attrs.put("size",new Integer(getPalette().getRows()));
  if (!palette.isPaletteEnabled()) {
    attrs.put("disabled","disabled");
  }
  getResponse().write(JavascriptUtils.SCRIPT_OPEN_TAG + "if (typeof(Wicket) != \"undefined\" && typeof(Wicket.Form) != \"undefined\")" + "    Wicket.Form.excludeFromAjaxSerialization."+ getMarkupId()+ "='true';"+ JavascriptUtils.SCRIPT_CLOSE_TAG);
}
