{
  Form form=new Form("form"){
    @Override protected void onSubmit(){
      super.onSubmit();
      result+="form";
    }
  }
;
  this.add(form);
  form.add(new Button("custom"){
    @Override public SubmitOrder getSubmitOrder(){
      return order;
    }
    @Override public void onSubmit(){
      super.onSubmit();
      result+="custom";
    }
  }
);
  form.add(new Button("default"){
    @Override public void onSubmit(){
      super.onSubmit();
      result+="default";
    }
  }
);
}
