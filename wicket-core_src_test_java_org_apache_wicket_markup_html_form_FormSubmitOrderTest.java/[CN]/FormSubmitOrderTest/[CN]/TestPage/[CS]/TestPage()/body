{
  Form form=new Form("form"){
    @Override protected void onSubmit(){
      super.onSubmit();
      result+="form";
    }
  }
;
  this.add(form);
  form.add(new Button("button"){
    @Override public void onSubmitBeforeForm(){
      super.onSubmitBeforeForm();
      result+="before";
    }
    @Override public void onSubmitAfterForm(){
      super.onSubmitAfterForm();
      result+="after";
    }
  }
);
}
