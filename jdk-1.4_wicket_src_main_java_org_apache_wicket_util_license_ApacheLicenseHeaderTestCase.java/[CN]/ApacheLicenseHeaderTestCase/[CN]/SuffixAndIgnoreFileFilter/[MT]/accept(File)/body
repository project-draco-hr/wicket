{
  boolean accept=false;
  if (pathname.isFile()) {
    if (ignoreFile(pathname) == false) {
      for (int i=0; i < suffixes.length; i++) {
        String suffix=suffixes[i];
        if (pathname.getName().endsWith("." + suffix)) {
          accept=true;
          break;
        }
      }
    }
  }
  return accept;
}
