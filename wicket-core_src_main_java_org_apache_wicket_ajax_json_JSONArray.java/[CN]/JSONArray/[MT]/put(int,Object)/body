{
  JSONObject.testValidity(value);
  if (index < 0) {
    throw new JSONException("JSONArray[" + index + "] not found.");
  }
  if (index < length()) {
    myArrayList.set(index,value);
  }
 else {
    while (index != length()) {
      put(JSONObject.NULL);
    }
    put(value);
  }
  return this;
}
