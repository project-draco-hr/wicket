{
  IModel<?> prevModel=getModelImpl();
  if (prevModel != null) {
    prevModel.detach();
  }
  IModel<?> wrappedModel=prevModel;
  if (prevModel instanceof IWrapModel) {
    wrappedModel=((IWrapModel<?>)prevModel).getWrappedModel();
  }
  if (wrappedModel != model) {
    modelChanging();
    setModelImpl(wrap(model));
    modelChanged();
  }
  return this;
}
