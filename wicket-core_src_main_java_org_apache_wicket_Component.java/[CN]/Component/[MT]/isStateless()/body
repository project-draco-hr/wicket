{
  if ((isVisibleInHierarchy() && isEnabledInHierarchy()) == false && canCallListenerInterface(null) == false) {
    return true;
  }
  if (!getStatelessHint()) {
    return false;
  }
  for (  Behavior behavior : getBehaviors()) {
    if (!behavior.getStatelessHint(this)) {
      return false;
    }
  }
  return true;
}
