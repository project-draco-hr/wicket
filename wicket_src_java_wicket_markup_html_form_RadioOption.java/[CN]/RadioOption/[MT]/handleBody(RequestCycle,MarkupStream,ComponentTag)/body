{
  Object value=getModelObject();
  final String displayLabel;
  if (label != null) {
    displayLabel=label;
  }
 else {
    displayLabel=String.valueOf(value);
  }
  String s=getLocalizer().getString(getPath() + "." + displayLabel,this,displayLabel);
  replaceBody(cycle,markupStream,openTag,s);
}
