{
  final AjaxEditableLabel<String> ajaxLabel=new AjaxEditableLabel<String>("ajaxLabel",new Model<String>("ajaxTest"));
  add(ajaxLabel);
  add(new AjaxLink<Void>("toggle"){
    private static final long serialVersionUID=1L;
    @Override public void onClick(    final AjaxRequestTarget target){
      ajaxLabel.setEnabled(!ajaxLabel.isEnabled());
      target.add(ajaxLabel);
    }
  }
);
}
