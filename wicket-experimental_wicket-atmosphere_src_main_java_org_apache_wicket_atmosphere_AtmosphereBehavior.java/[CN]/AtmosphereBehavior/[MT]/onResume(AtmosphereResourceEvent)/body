{
  if (log.isDebugEnabled()) {
    String transport=event.getResource().getRequest().getHeader("X-Atmosphere-Transport");
    HttpServletRequest req=event.getResource().getRequest();
    log.debug(String.format("Resuming the %s response from ip %s:%s",transport == null ? "websocket" : transport,req.getRemoteAddr(),req.getRemotePort()));
  }
}
