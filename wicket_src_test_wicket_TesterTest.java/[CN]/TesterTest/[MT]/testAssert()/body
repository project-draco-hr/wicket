{
  tester.startPage(new ITestPageSource(){
    public Page getTestPage(){
      return new MyPage();
    }
  }
);
  tester.debugComponentTrees();
  try {
    tester.assertVisible("label");
    fail("Should fail, because label is invisible");
  }
