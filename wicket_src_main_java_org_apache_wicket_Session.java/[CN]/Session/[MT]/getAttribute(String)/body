{
  if (!isTemporary()) {
    RequestCycle cycle=RequestCycle.get();
    if (cycle != null) {
      return (Serializable)getSessionStore().getAttribute(cycle.getRequest(),name);
    }
  }
 else {
    if (temporarySessionAttributes != null) {
      return (Serializable)temporarySessionAttributes.get(name);
    }
  }
  return null;
}
