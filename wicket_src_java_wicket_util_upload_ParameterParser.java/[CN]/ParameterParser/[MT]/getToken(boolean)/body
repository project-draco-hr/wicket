{
  while ((i1 < i2) && (Character.isWhitespace(chars[i1]))) {
    i1++;
  }
  while ((i2 > i1) && (Character.isWhitespace(chars[i2 - 1]))) {
    i2--;
  }
  if (quoted) {
    if (((i2 - i1) >= 2) && (chars[i1] == '"') && (chars[i2 - 1] == '"')) {
      i1++;
      i2--;
    }
  }
  String result=null;
  if (i2 > i1) {
    result=new String(chars,i1,i2 - i1);
  }
  return result;
}
