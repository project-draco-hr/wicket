{
  if (value == null) {
    return null;
  }
  if (c == CONVERT_TO_DEFAULT_TYPE || Number.class.isAssignableFrom(c) || c == Float.TYPE) {
    Number temp=getNumber(value);
    return (temp instanceof Float) ? (Float)temp : new Float(temp.floatValue());
  }
 else   if (String.class.isAssignableFrom(c)) {
    return toString(value);
  }
  throw new ConversionException(this + " cannot handle conversions of type " + c);
}
