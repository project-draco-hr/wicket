{
  super.renderHead(component,response);
  response.render(JavaScriptHeaderItem.forReference(CHOICE_JS));
  String onLoadScript=String.format("Wicket.Choice.attach('%s', '%s', %s)",component.getMarkupId(),getFormComponent().getInputName(),renderAjaxAttributes(component));
  response.render(OnLoadHeaderItem.forScript(onLoadScript));
}
