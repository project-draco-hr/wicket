{
  ResourceReferenceAttributes res=new ResourceReferenceAttributes();
  if (!Strings.isEmpty(attributes)) {
    String split[]=attributes.split("-",3);
    res.locale=parseLocale(split[0]);
    if (split.length == 2) {
      res.style=split[1];
    }
 else     if (split.length == 3) {
      res.style=split[1];
      res.variation=split[2];
    }
  }
  return res;
}
