{
  Locale locale=null;
  String style=null;
  String variation=null;
  if (!Strings.isEmpty(attributes)) {
    String split[]=attributes.split("-",3);
    locale=parseLocale(split[0]);
    if (split.length == 2) {
      style=split[1];
    }
 else     if (split.length == 3) {
      style=split[1];
      variation=split[2];
    }
  }
  return new ResourceReference.UrlAttributes(locale,style,variation);
}
