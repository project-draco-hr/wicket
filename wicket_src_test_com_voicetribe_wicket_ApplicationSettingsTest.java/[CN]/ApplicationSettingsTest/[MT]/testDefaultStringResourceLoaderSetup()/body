{
  ApplicationSettings settings=new ApplicationSettings(new DummyApplication());
  List loaders=settings.getStringResourceLoaders();
  Assert.assertEquals("There should be 2 default loaders",2,loaders.size());
  Assert.assertTrue("First loader one should be the component one",loaders.get(0) instanceof ComponentStringResourceLoader);
  Assert.assertTrue("Second loader should be the application one",loaders.get(1) instanceof ApplicationStringResourceLoader);
}
