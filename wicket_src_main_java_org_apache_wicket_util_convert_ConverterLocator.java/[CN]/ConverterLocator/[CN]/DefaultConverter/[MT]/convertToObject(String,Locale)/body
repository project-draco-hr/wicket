{
  if (value == null) {
    return null;
  }
  Class<T> theType=type.get();
  if ("".equals(value)) {
    if (theType.equals(String.class)) {
      return theType.cast("");
    }
    return null;
  }
  try {
    return Objects.convertValue(value,theType);
  }
 catch (  Exception e) {
    throw new ConversionException(e.getMessage(),e).setSourceValue(value);
  }
}
