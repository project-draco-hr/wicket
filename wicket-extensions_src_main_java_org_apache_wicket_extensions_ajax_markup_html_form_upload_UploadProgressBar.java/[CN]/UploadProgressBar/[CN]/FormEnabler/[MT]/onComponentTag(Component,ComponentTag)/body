{
  ResourceReference ref=new SharedResourceReference(RESOURCE_NAME);
  final String uploadFieldId=(uploadField == null) ? "" : uploadField.getMarkupId();
  tag.put("onsubmit","var def=new Wicket.WUPB.Def('" + component.getMarkupId() + "', '"+ status.getMarkupId()+ "', '"+ bar.getMarkupId()+ "', '"+ component.getPage().urlFor(ref,null)+ "','"+ uploadFieldId+ "'); Wicket.WUPB.start(def);");
