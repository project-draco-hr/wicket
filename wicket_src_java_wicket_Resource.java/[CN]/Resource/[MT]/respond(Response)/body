{
  try {
    final OutputStream out=response.getOutputStream();
    try {
      Streams.copy(resourceStream.getInputStream(),out);
    }
  finally {
      resourceStream.close();
      out.flush();
    }
  }
 catch (  Exception e) {
    throw new WicketRuntimeException("Unable to render resource stream " + resourceStream,e);
  }
}
