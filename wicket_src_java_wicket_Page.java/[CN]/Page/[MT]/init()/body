{
  final RequestCycle cycle=getRequestCycle();
  String pageMapName=null;
  if (cycle != null) {
    RequestParameters parameters=cycle.getProcessor().getRequestCodingStrategy().decode(cycle.getRequest());
    pageMapName=parameters.getPageMapName();
  }
  final PageMap pageMap=PageMap.forName(pageMapName);
  init(pageMap);
}
