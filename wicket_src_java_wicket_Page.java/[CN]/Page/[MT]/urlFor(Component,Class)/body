{
  RequestCycle requestCycle=getRequestCycle();
  String interfaceName=Classes.name(listenerInterface);
  IRequestEncoder requestEncoder=requestCycle.getRequestCycleProcessor().getRequestEncoder();
  String url=requestEncoder.encode(requestCycle,new ListenerInterfaceRequestTarget(this,component,requestCycle.getRequestInterfaceMethod(interfaceName)));
  return url;
}
