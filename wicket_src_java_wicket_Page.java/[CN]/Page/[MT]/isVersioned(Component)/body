{
  if (component.isAuto()) {
    return false;
  }
 else {
    if (getFlag(FLAG_IS_RENDERING)) {
      throw new WicketRuntimeException("Cannot modify component hierarchy during render phase");
    }
    if (getFlag(FLAG_TRACK_CHANGES) && component.isVersioned()) {
      newVersion();
      return true;
    }
    return false;
  }
}
