{
  if (checkAccess()) {
    try {
      visitChildren(IFeedback.class,new IVisitor(){
        public Object component(        Component component){
          component.internalBeginRequest();
          return IVisitor.CONTINUE_TRAVERSAL;
        }
      }
);
      internalBeginRequest();
      render();
      checkRendering();
    }
  finally {
      internalEndRequest();
    }
  }
}
