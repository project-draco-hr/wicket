{
  if (locale == null) {
    locale=Session.get().getLocale();
    if (locale == null) {
      locale=Locale.getDefault();
    }
  }
  try {
    final ResourceBundle bundle=ResourceBundle.getBundle(bundleName,locale);
    return bundle.getString(key);
  }
 catch (  MissingResourceException e) {
    return null;
  }
}
