{
  assertFalse(StringValue.valueOf("bla",Locale.FRANCE).equals(StringValue.valueOf("bla",Locale.CANADA)));
  assertTrue(StringValue.valueOf("bla",Locale.FRANCE).equals(StringValue.valueOf("bla",Locale.FRANCE)));
  assertFalse(StringValue.valueOf("bla",Locale.FRANCE).equals(StringValue.valueOf("blo",Locale.FRANCE)));
}
