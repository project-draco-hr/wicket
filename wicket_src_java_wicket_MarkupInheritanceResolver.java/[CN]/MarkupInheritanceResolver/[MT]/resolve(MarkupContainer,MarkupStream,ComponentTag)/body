{
  if (tag instanceof WicketTag) {
    final WicketTag wicketTag=(WicketTag)tag;
    if (wicketTag.isExtendTag()) {
      container.getResponse().write(wicketTag);
      markupStream.next();
      return true;
    }
    if (wicketTag.isChildTag()) {
      container.getResponse().write(wicketTag);
      markupStream.next();
      return true;
    }
  }
  return false;
}
