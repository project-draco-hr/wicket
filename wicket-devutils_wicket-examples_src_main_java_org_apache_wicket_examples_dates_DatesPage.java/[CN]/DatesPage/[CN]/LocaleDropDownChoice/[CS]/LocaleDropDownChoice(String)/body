{
  super(id);
  setChoices(new AbstractReadOnlyModel<List<? extends Locale>>(){
    @Override public List<Locale> getObject(){
      getSelectedLocale();
      List<Locale> locales=new ArrayList<Locale>(LOCALES);
      Collections.sort(locales,new Comparator<Locale>(){
        public int compare(        Locale o1,        Locale o2){
          return o1.getDisplayName(selectedLocale).compareTo(o2.getDisplayName(selectedLocale));
        }
      }
);
      return locales;
    }
  }
);
  setChoiceRenderer(new LocaleChoiceRenderer());
  setDefaultModel(new PropertyModel<Locale>(DatesPage.this,"selectedLocale"));
}
