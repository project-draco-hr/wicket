{
  if (callComponentListener() || bookmarkablePage() || homePage()) {
    final Page page=getPage();
    if (page != null) {
      if (getRedirect()) {
        redirectToPage(page);
      }
 else {
        page.render();
        page.getFeedbackMessages().clear();
      }
    }
  }
 else {
    if (!renderStaticContent()) {
      response.write("<pre>Invalid request: " + request + "</pre>");
    }
  }
}
