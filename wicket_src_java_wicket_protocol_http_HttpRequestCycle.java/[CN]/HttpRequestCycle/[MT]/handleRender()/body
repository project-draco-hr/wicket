{
  if (callComponentListener() || bookmarkablePage() || homePage()) {
    Page page=getPage();
    if (page != null) {
      if (getRedirect()) {
        redirectToPage(page);
      }
 else {
        page.render(this);
      }
    }
  }
 else   if (respondWithStaticContent()) {
  }
 else {
    response.write("<pre>Invalid request: " + request + "</pre>");
  }
}
