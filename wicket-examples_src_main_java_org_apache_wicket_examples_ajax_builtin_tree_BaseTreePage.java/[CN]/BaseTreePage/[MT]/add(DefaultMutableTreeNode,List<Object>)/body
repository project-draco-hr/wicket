{
  for (  Object obj : sub) {
    if (obj instanceof List) {
      DefaultMutableTreeNode child=new DefaultMutableTreeNode(new ModelBean("subtree..."));
      parent.add(child);
      add(child,(List<Object>)obj);
    }
 else {
      DefaultMutableTreeNode child=new DefaultMutableTreeNode(new ModelBean(obj.toString()));
      parent.add(child);
    }
  }
}
