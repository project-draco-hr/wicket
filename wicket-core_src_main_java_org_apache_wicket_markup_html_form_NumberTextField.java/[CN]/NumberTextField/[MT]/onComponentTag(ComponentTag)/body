{
  super.onComponentTag(tag);
  IValueMap attributes=tag.getAttributes();
  if (minimum != null) {
    attributes.put("min",Objects.stringValue(minimum));
  }
 else {
    attributes.remove("min");
  }
  if (maximum != null) {
    attributes.put("max",Objects.stringValue(maximum));
  }
 else {
    attributes.remove("max");
  }
}
