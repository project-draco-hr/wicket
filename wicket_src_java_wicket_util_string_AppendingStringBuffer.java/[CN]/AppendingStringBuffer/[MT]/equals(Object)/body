{
  if (this == anObject) {
    return true;
  }
  if (anObject instanceof AppendingStringBuffer) {
    AppendingStringBuffer anotherString=(AppendingStringBuffer)anObject;
    int n=count;
    if (n == anotherString.count) {
      char v1[]=value;
      char v2[]=anotherString.value;
      int i=0;
      int j=0;
      while (n-- != 0) {
        if (v1[i++] != v2[j++]) {
          return false;
        }
      }
      return true;
    }
  }
  return false;
}
