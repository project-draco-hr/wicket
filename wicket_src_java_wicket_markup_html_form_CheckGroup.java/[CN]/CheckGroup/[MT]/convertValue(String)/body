{
  String[] paths=inputAsStringArray();
  List collection=new ArrayList();
  if (paths != null && paths.length > 0) {
    for (int i=0; i < paths.length; i++) {
      String path=paths[i];
      if (path != null) {
        path=path.substring(getPath().length() + 1);
        Check checkbox=(Check)get(path);
        if (checkbox == null) {
          throw new WicketRuntimeException("submitted http post value [" + paths.toString() + "] for CheckGroup component ["+ getPath()+ "] contains an illegal relative path "+ "element ["+ path+ "] which does not point to a Check component. Due to this the CheckGroup component cannot resolve the selected Check component pointed to by the illegal value. A possible reason is that componment hierarchy changed between rendering and form submission.");
        }
        collection.add(checkbox.getModelObject());
      }
    }
  }
  return collection;
}
