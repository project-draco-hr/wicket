{
  if (this.associatedMarkup == null) {
    try {
      this.associatedMarkup=getApplication().getMarkupCache().getMarkup(this,throwException);
    }
 catch (    MarkupException ex) {
      throw ex;
    }
catch (    WicketRuntimeException ex) {
      throw new MarkupNotFoundException(exceptionMessage("Markup of type '" + getMarkupType() + "' for component '"+ getClass().getName()+ "' not found."+ " Enable debug messages for wicket.util.resource to get a list of all filenames tried"),ex);
    }
    onAssociatedMarkupLoaded(this.associatedMarkup);
  }
  return this.associatedMarkup;
}
