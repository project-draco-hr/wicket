{
  super.attachChildren();
  try {
    for (    Component child : this) {
      child.attach();
    }
  }
 catch (  RuntimeException ex) {
    if (ex instanceof WicketRuntimeException) {
      throw ex;
    }
 else {
      throw new WicketRuntimeException("Error attaching this container for rendering: " + this,ex);
    }
  }
}
