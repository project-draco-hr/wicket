{
  if (pageInfo == null) {
    return url;
  }
 else {
    if (url.indexOf('?') == -1) {
      return url + getBeginSeparator() + pageInfo.toString()+ getEndSeparator();
    }
 else {
      return Strings.firstPathComponent(url,'?') + getBeginSeparator() + pageInfo.toString()+ getEndSeparator()+ '?'+ Strings.afterFirstPathComponent(url,'?');
    }
  }
}
