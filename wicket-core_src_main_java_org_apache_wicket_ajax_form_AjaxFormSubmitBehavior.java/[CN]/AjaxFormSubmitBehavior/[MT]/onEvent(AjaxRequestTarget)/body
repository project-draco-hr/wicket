{
  getForm().getRootForm().onFormSubmitted(new IFormSubmitter(){
    @Override public Form<?> getForm(){
      return AjaxFormSubmitBehavior.this.getForm();
    }
    @Override public boolean getDefaultFormProcessing(){
      return AjaxFormSubmitBehavior.this.getDefaultProcessing();
    }
    @Override public void onError(){
      AjaxFormSubmitBehavior.this.onError(target);
    }
    @Override public void onSubmitBeforeForm(){
      AjaxFormSubmitBehavior.this.onSubmitBeforeForm(target);
    }
    @Override public void onSubmitAfterForm(){
      AjaxFormSubmitBehavior.this.onSubmitAfterForm(target);
    }
  }
);
}
