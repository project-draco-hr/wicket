{
  if (WICKET_RELATIVE_PATH_PREFIX_CONTAINER_ID.equals(tag.getId())) {
    final Component<?> wc;
    String id=WICKET_RELATIVE_PATH_PREFIX_CONTAINER_ID + container.getPage().getAutoIndex();
    if (tag.isOpenClose()) {
      wc=new WebComponent<Object>(id);
    }
 else {
      wc=new WebMarkupContainer<Object>(id){
        private static final long serialVersionUID=1L;
        @Override public boolean isTransparentResolver(){
          return true;
        }
      }
;
    }
    container.autoAdd(wc,markupStream);
    return true;
  }
  return false;
}
