{
  if (WICKET_RELATIVE_PATH_PREFIX_CONTAINER_ID.equals(tag.getId())) {
    Component wc=null;
    if (tag.isOpenClose()) {
      wc=new WebComponent(WICKET_RELATIVE_PATH_PREFIX_CONTAINER_ID + container.getPage().getAutoIndex());
    }
 else {
      wc=new WebMarkupContainer(WICKET_RELATIVE_PATH_PREFIX_CONTAINER_ID + container.getPage().getAutoIndex());
    }
    wc.add(RELATIVE_PATH_BEHAVIOR);
    container.autoAdd(wc);
    return true;
  }
  return false;
}
