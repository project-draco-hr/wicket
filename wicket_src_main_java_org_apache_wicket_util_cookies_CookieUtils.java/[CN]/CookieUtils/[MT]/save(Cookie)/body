{
  if (cookie == null) {
    return null;
  }
  initializeCookie(cookie);
  getWebResponse().addCookie(cookie);
  if (log.isDebugEnabled()) {
    log.debug("Cookie saved: " + cookieToDebugString(cookie) + "; request URI="+ getWebRequest().getHttpServletRequest().getRequestURI());
  }
  return cookie;
}
