{
  String key=getSaveKey(name);
  try {
    Cookie cookie=getWebRequest().getCookie(key);
    if (log.isDebugEnabled()) {
      if (cookie != null) {
        log.debug("Found Cookie with name=" + key + " and request URI="+ getWebRequest().getUrl().toString());
      }
 else {
        log.debug("Unable to find Cookie with name=" + key + " and request URI="+ getWebRequest().getUrl().toString());
      }
    }
    return cookie;
  }
 catch (  NullPointerException ex) {
  }
  return null;
}
