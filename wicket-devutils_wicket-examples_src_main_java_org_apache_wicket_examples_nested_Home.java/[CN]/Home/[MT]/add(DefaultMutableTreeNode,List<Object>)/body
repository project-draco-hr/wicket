{
  for (Iterator<Object> i=sub.iterator(); i.hasNext(); ) {
    Object o=i.next();
    if (o instanceof List) {
      DefaultMutableTreeNode child=new DefaultMutableTreeNode(o);
      parent.add(child);
      add(child,(List<Object>)o);
    }
 else {
      DefaultMutableTreeNode child=new DefaultMutableTreeNode(o);
      parent.add(child);
    }
  }
}
