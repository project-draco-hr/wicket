{
  int ixExtension=path.lastIndexOf('.');
  int len=path.length();
  final String ext;
  if (ixExtension <= 0 || ixExtension == len || (path.lastIndexOf('/') + 1) == ixExtension) {
    ext=null;
  }
 else {
    ext=path.substring(ixExtension + 1).toLowerCase();
  }
  return acceptExtension(ext);
}
