{
  super(id);
  setOutputMarkupId(true);
  add(new ListView<TodoItem>("item",items){
    @Override protected void populateItem(    ListItem<TodoItem> item){
      item.add(new AjaxCheckBox("check",new PropertyModel<Boolean>(item.getDefaultModel(),"checked")){
        @Override protected void onUpdate(        AjaxRequestTarget target){
        }
      }
);
      item.add(new Label("text",new PropertyModel<String>(item.getDefaultModel(),"text")));
    }
  }
);
}
