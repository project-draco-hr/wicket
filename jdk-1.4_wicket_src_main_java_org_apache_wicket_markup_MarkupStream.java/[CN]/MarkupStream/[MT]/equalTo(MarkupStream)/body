{
  while (this.hasMore()) {
    final MarkupElement thisElement=this.get();
    final MarkupElement thatElement=that.get();
    if (thisElement != null && thatElement != null) {
      if (!thisElement.equalTo(thatElement)) {
        return false;
      }
    }
 else {
      if (!(thisElement == null && thatElement == null)) {
        return false;
      }
    }
    this.next();
    that.next();
  }
  if (!that.hasMore()) {
    return true;
  }
  return false;
}
