{
  assertNull(Strings.afterLast(null,'.'));
  assertEquals("",Strings.afterLast("",'.'));
  assertEquals("",Strings.afterLast("",' '));
  assertEquals("",Strings.afterLast(".",'.'));
  assertEquals("",Strings.afterLast("..",'.'));
  assertEquals("bar",Strings.afterLast("com.foo.bar",'.'));
  assertEquals("bar",Strings.afterLast("com foo bar",' '));
  assertEquals("bar",Strings.afterLast("com foo.bar",'.'));
}
