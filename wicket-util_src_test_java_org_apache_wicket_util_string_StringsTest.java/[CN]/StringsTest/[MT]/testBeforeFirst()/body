{
  assertNull(Strings.beforeFirst(null,'.'));
  assertEquals("",Strings.beforeFirst("",'.'));
  assertEquals("",Strings.beforeFirst("",' '));
  assertEquals("",Strings.beforeFirst(".",'.'));
  assertEquals("",Strings.beforeFirst("..",'.'));
  assertEquals("com",Strings.beforeFirst("com.foo.bar",'.'));
  assertEquals("com",Strings.beforeFirst("com foo bar",' '));
  assertEquals("com foo",Strings.beforeFirst("com foo.bar",'.'));
}
