{
  for (  EventSubscription curSubscription : subscriptions) {
    if (curSubscription.getContextAwareFilter().apply(event)) {
      Component component=page.get(curSubscription.getComponentPath());
      if (curSubscription.getBehaviorIndex() == null)       invokeMethod(target,curSubscription,component);
 else       invokeMethod(target,curSubscription,component.getBehaviorById(curSubscription.getBehaviorIndex()));
    }
  }
}
