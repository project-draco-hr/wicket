{
  try {
    locator.locateProxyTarget();
  }
 catch (  Throwable e) {
    String errorMessage="Could not locate spring bean of class [[" + locator.getBeanType().getName() + "]] ";
    if (locator.getBeanId() != null && locator.getBeanId().length() > 0) {
      errorMessage+="and id [[" + locator.getBeanId() + "]] ";
    }
    errorMessage+="needed in class [[" + fieldOwner.getClass().getName() + "]] field [["+ field.getName()+ "]]";
    throw new RuntimeException(errorMessage,e);
  }
}
