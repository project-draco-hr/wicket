{
  String packageName=Packages.extractPackageName(page.getClass());
  while ("".equals(packageName) == false) {
    final PageSet pageSet=(PageSet)packageNameToPageSet.get(packageName);
    if (pageSet != null) {
      return pageSet;
    }
    packageName=Packages.parent(packageName);
  }
  return null;
}
