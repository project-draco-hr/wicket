{
  final ImagesApplication application=(ImagesApplication)getApplication();
  new Image(this,"image2");
  new Image(this,"image3",new RenderedDynamicImageResource(100,100){
    @Override protected boolean render(    Graphics2D graphics){
      drawCircle(graphics);
