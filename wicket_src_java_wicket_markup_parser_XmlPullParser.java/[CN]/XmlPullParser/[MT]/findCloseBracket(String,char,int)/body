{
  char quote=0;
  for (; startIndex < input.length(); startIndex++) {
    char charAt=input.charAt(startIndex);
    if (quote != 0) {
      if (quote == charAt) {
        quote=0;
      }
    }
 else     if ((charAt == '"') || (charAt == '\'')) {
      quote=charAt;
    }
 else     if (charAt == ch) {
      return startIndex;
    }
  }
  return -1;
}
