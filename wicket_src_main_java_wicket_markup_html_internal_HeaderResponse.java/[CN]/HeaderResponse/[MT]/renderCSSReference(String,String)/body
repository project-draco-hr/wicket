{
  List token=Arrays.asList(new Object[]{"css",url,media});
  if (wasRendered(token) == false) {
    response.write("<link rel=\"stylesheet\" type=\"text/css\" href=\"");
    response.write(url);
    response.write("\"");
    if (media != null) {
      response.write(" media=\"");
      response.write(media);
      response.write("\"");
    }
    response.println(" />");
    markRendered(token);
  }
}
