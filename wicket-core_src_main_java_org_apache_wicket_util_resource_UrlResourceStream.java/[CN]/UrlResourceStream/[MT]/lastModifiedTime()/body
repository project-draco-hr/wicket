{
  try {
    final Time time=Connections.getLastModified(url);
    if (Objects.equal(time,lastModified) == false) {
      lastModified=time;
      this.updateContentLength();
    }
    return lastModified;
  }
 catch (  IOException e) {
    log.warn("getLastModified for " + url + " failed: "+ e.getMessage());
    return null;
  }
}
