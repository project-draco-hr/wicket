{
  ComponentTag tag=(ComponentTag)getParent().nextTag();
  if (tag == null) {
    return tag;
  }
  if (ignoreRemainingTag == true) {
    return null;
  }
  if (tag instanceof WicketTag) {
    final WicketTag wtag=(WicketTag)tag;
    if (wtag.isExtendTag()) {
      if (wtag.isClose()) {
        ignoreRemainingTag=true;
      }
 else {
        tagList.add(null);
      }
    }
  }
  return tag;
}
