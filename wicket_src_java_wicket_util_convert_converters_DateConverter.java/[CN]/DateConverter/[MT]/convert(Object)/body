{
  final String stringValue=value.toString();
  try {
    return dateFormat.parse(stringValue);
  }
 catch (  ParseException e) {
    throw new ConversionException("Cannot convert '" + stringValue + "' to Date",e);
  }
catch (  NumberFormatException e) {
    throw new ConversionException("Cannot convert '" + stringValue + "' to Date",e);
  }
}
