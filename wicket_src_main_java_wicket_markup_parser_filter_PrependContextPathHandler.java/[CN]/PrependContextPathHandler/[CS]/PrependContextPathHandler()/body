{
  setPriority(999);
  String contextPath=Application.get().getApplicationSettings().getContextPath();
  if (contextPath != null) {
    if (contextPath.length() == 0) {
      contextPath=null;
    }
 else     if (contextPath.endsWith("/") == false) {
      contextPath+="/";
    }
  }
  this.contextPath=contextPath;
}
