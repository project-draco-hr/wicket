{
  return new ICrypt(){
    @Override public String decryptUrlSafe(    String text){
      return new String(new Base64(true).decode(text));
    }
    @Override public String encryptUrlSafe(    String plainText){
      return new String(new Base64(true).encode(plainText.getBytes()));
    }
    @Override public void setKey(    String key){
    }
  }
;
}
