{
  try {
    return getMapper().writeValueAsString(new RequestSessionTuple(rd,sd));
  }
 catch (  JsonGenerationException e) {
    throw new RuntimeException(e);
  }
catch (  JsonMappingException e) {
    throw new RuntimeException(e);
  }
catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
