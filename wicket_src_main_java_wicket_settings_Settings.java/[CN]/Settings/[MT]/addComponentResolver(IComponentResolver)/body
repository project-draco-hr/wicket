{
  boolean hit=false;
  for (  final IComponentResolver componentResolver : this.componentResolvers) {
    if (componentResolver.getClass().equals(resolver.getClass())) {
      hit=true;
      break;
    }
  }
  if (hit == false) {
    componentResolvers.add(resolver);
  }
 else {
    log.warn("A IComponentResolver of type " + resolver.getClass().getName() + " has already been registered. The new one will not be added");
  }
}
