{
  VariableAssignmentParser parser=new VariableAssignmentParser("foo = 9");
  assertTrue(parser.matches());
  assertEquals("foo",parser.getKey());
  assertEquals("9",parser.getValue());
  parser=new VariableAssignmentParser("foo=9");
  assertTrue(parser.matches());
  assertEquals("foo",parser.getKey());
  assertEquals("9",parser.getValue());
}
