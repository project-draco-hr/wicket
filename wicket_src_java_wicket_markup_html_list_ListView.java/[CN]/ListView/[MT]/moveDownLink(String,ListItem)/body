{
  final int index=getList().indexOf(item.getModelObject());
  final Link link=new Link(id){
    public void onClick(){
      Collections.swap(getList(),index,index + 1);
      ListView.this.removeAll();
    }
  }
;
  if (index == (getList().size() - 1)) {
    link.setVisible(false);
  }
  return link;
}
