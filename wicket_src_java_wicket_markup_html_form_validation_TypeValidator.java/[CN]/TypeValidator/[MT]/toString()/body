{
  StringBuffer b=new StringBuffer("TypeValidator{type=").append(getType());
  if (isUseLocalized()) {
    Locale loc=getLocale();
    if (loc != null) {
      b.append(",locale=").append(getLocale());
    }
 else {
      b.append(",(use client locale)");
    }
  }
  b.append("}");
  return b.toString();
}
