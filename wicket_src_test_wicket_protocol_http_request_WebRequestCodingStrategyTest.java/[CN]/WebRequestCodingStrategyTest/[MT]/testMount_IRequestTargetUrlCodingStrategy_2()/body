{
  try {
    WebRequestCodingStrategy strategy=new WebRequestCodingStrategy();
    strategy.mount(new MockRequestTargetUrlCodingStrategy(){
      @Override public String getMountPath(){
        return null;
      }
    }
);
  }
 catch (  IllegalArgumentException e) {
    if ("Argument path must be not be empty".equals(e.getMessage()) == false) {
      fail();
    }
  }
catch (  Exception e) {
    fail();
  }
  try {
    WebRequestCodingStrategy strategy=new WebRequestCodingStrategy();
    strategy.mount(new MockRequestTargetUrlCodingStrategy(){
      @Override public String getMountPath(){
        return "";
      }
    }
);
  }
 catch (  IllegalArgumentException e) {
    if ("Argument path must be not be empty".equals(e.getMessage()) == false) {
      fail();
    }
  }
catch (  Exception e) {
    fail();
  }
  try {
    WebRequestCodingStrategy strategy=new WebRequestCodingStrategy();
    strategy.mount(new MockRequestTargetUrlCodingStrategy(){
      @Override public String getMountPath(){
        return "/";
      }
    }
);
  }
 catch (  IllegalArgumentException e) {
    if ("The mount path '/' is reserved for the application home page".equals(e.getMessage()) == false) {
      fail();
    }
  }
catch (  Exception e) {
    fail();
  }
}
