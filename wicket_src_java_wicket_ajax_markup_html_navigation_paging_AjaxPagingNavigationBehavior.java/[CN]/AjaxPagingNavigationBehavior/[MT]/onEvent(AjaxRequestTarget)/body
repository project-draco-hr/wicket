{
  owner.onClick(target);
  Component container=((Component<?>)pageable).findParent(MarkupContainer.class);
  target.addComponent(container);
  Component navigator=(AjaxPagingNavigator)((Component<?>)owner).findParent(AjaxPagingNavigator.class);
  if (navigator == null) {
    navigator=(Component)owner;
  }
  if (navigator != null) {
    if (!(container instanceof MarkupContainer && ((MarkupContainer)container).contains(navigator,true))) {
      target.addComponent(navigator);
    }
  }
}
