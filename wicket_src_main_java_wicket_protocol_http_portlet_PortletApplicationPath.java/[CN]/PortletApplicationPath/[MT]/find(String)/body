{
  for (  Folder folder : folders) {
    File file=new File(folder,pathname);
    if (file.exists()) {
      try {
        return file.toURI().toURL();
      }
 catch (      MalformedURLException ex) {
      }
    }
  }
  for (  String path : webappPaths) {
    try {
      final URL file=portletContext.getResource(path + pathname);
      if (file != null) {
        return file;
      }
    }
 catch (    Exception ex) {
    }
  }
  return null;
}
