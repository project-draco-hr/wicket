{
  String value=getValue();
  getResponse().write("<ul>\n");
  String[] results=getResults(value);
  for (int x=0; x < results.length; x++) {
    String result=results[x];
    getResponse().write("<li>");
    getResponse().write(result);
    getResponse().write("</li>\n");
  }
  getResponse().write("</ul>\n");
}
