{
  add(new StockQuoteLabel("stockIBM","IBM"));
  add(new StockQuoteLabel2("stock2IBM","IBM"));
  final IModel model=new PropertyModel(quote,"symbol");
  final Form form=new Form("form");
  add(form);
  form.add(new TextField("symbol",model));
