{
  final String img;
  if (node.isRoot()) {
    img="filebrowser/folderopen.gif";
  }
 else   if (node.isLeaf()) {
    img="filebrowser/node.gif";
  }
 else {
    if (node.isExpanded()) {
      img="filebrowser/folderopen.gif";
    }
 else {
      img="filebrowser/folder.gif";
    }
  }
  return img;
}
