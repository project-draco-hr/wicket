{
  super.renderHead(component,response);
  IPartialPageRequestHandler target=component.getRequestCycle().find(IPartialPageRequestHandler.class);
  if (target != null) {
    final String javascript="var e = Wicket.$('" + getMarkupId() + "'); if (e != null && typeof(e.parentNode) != 'undefined') e.parentNode.removeChild(e);";
    target.prependJavaScript(javascript);
  }
}
