{
  return new AbstractSet<K>(){
    public Iterator<K> iterator(){
      return new Iterator<K>(){
        public boolean hasNext(){
          return index < MicroMap.this.size();
        }
        public K next(){
          index++;
          return key;
        }
        public void remove(){
          MicroMap.this.clear();
        }
        int index;
      }
;
    }
    public int size(){
      return MicroMap.this.size();
    }
  }
;
}
