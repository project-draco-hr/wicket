{
  return new Iterator(){
    int index=startIndex - 1;
    public boolean hasNext(){
      while (++index < size()) {
        MarkupElement element=get(index);
        if ((matchClass == null) || matchClass.isInstance(element)) {
          return true;
        }
      }
      return false;
    }
    public Object next(){
      return get(index);
    }
    public void remove(){
      markupElements.remove(index);
      index-=1;
    }
  }
;
}
