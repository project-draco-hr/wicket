{
  AppendingStringBuffer asb=new AppendingStringBuffer(super.getMessage());
  if (list.size() > 0) {
    asb.append("\n");
    for (int i=list.size(); --i >= 0; ) {
      String element=(String)list.get(i);
      asb.append(element);
      asb.append("->");
    }
    asb.setLength(asb.length() - 2);
  }
  return asb.toString();
}
