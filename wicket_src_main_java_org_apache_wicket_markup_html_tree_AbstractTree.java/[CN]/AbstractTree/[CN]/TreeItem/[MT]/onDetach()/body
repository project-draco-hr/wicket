{
  super.onDetach();
  Object object=getDefaultModelObject();
  if (object instanceof IDetachable) {
    ((IDetachable)object).detach();
  }
  if (isRenderChildren()) {
    visitItemChildren(this,new IItemCallback(){
      public void visitItem(      TreeItem item){
        item.detach();
      }
    }
);
  }
  setRenderChildren(false);
}
