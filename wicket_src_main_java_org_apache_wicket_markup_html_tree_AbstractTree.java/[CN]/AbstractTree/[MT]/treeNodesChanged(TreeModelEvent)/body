{
  if (dirtyAll) {
    return;
  }
  if (e.getChildren() == null) {
    if (rootItem != null) {
      invalidateNode(rootItem.getModelObject(),true);
    }
  }
 else {
    Object[] children=e.getChildren();
    if (children != null) {
      for (      Object node : children) {
        if (isNodeVisible(node)) {
          invalidateNode(node,true);
        }
      }
    }
  }
}
