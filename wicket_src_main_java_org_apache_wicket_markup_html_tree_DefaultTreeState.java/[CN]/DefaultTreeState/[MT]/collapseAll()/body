{
  if (nodes.isEmpty() && nodesCollapsed == false) {
  }
 else {
    nodes.clear();
    nodesCollapsed=false;
    Object[] listenersCopy=listeners.toArray();
    for (int i=0; i < listenersCopy.length; i++) {
      ITreeStateListener l=(ITreeStateListener)listenersCopy[i];
      l.allNodesCollapsed();
    }
  }
}
