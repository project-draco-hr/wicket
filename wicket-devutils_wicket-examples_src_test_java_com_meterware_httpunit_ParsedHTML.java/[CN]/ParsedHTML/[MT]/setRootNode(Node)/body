{
  if (_rootNode != null && rootNode != _rootNode) {
    throw new IllegalStateException("The root node has already been defined as " + _rootNode + " and cannot be redefined as "+ rootNode);
  }
  _rootNode=rootNode;
  _links=null;
  _forms=null;
  _images=null;
  _applets=null;
  _tables=null;
  _frames=null;
  _blocks=null;
  _updateElements=true;
}
