{
  if (!isVisible()) {
    findMarkupStream().skipComponent();
  }
 else {
    final RequestCycle cycle=getRequestCycle();
    final Response originalResponse;
    originalResponse=null;
    if (log.isDebugEnabled()) {
      log.debug("Begin render " + this);
    }
    onRender();
    rendered();
    if (log.isDebugEnabled()) {
      log.debug("End render " + this);
    }
  }
}
