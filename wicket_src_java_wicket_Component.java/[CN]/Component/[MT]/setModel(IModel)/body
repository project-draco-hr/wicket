{
  if (this.model != null && this.model instanceof IDetachableModel) {
    ((IDetachableModel)this.model).detach();
  }
  if (model instanceof IConvertible) {
    ((IConvertible)model).setConverterSource(this);
  }
  if (this.model != model) {
    this.model=(IModel)model;
    modelChanged();
  }
}
