{
  if (behaviourListener == null) {
    throw new NullPointerException("argument behaviourListener must be not null");
  }
  if (behaviours == null) {
    throw new IllegalArgumentException("behaviourListener " + behaviourListener + " was not registered with this component");
  }
  int index=behaviours.indexOf(behaviourListener);
  if (index == -1) {
    throw new IllegalArgumentException("behaviourListener " + behaviourListener + " was not registered with this component");
  }
  return urlFor(IBehaviourListener.class) + "&behaviourId=" + index;
}
