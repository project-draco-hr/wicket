{
  final IModel model=getModel();
  if (model != null) {
    if (getFlag(FLAG_HAS_ROOT_MODEL)) {
      Object result=getRootModel(model);
      if ((result instanceof IModel)) {
        result=((IModel)result).getObject(this);
      }
      return result;
    }
    return model.getObject(this);
  }
 else {
    return null;
  }
}
