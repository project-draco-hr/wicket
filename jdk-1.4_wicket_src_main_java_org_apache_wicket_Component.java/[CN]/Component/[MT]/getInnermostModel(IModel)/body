{
  IModel nested=model;
  while (nested != null && nested instanceof IWrapModel) {
    final IModel next=((IWrapModel)nested).getWrappedModel();
    if (nested == next) {
      throw new WicketRuntimeException("Model for " + nested + " is self-referential");
    }
    nested=next;
  }
  return nested;
}
