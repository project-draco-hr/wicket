{
  if (!(this instanceof IFeedback)) {
    internalBeforeRender();
  }
 else {
    List feedbacks=(List)getRequestCycle().getMetaData(FEEDBACK_LIST);
    if (feedbacks == null) {
      feedbacks=new ArrayList();
      getRequestCycle().setMetaData(FEEDBACK_LIST,(Serializable)feedbacks);
    }
    feedbacks.add(this);
  }
}
