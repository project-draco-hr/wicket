{
  if (behavior == null) {
    throw new IllegalArgumentException("Argument may not be null");
  }
  if (behaviors == null) {
    behaviors=new ArrayList(1);
  }
  behaviors.add(behavior);
  if (!behavior.isTemporary()) {
    addStateChange(new AddedBehaviorChange(behavior));
  }
  behavior.bind(this);
  return this;
}
