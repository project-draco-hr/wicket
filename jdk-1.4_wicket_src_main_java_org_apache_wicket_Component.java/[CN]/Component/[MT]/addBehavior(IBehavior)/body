{
  if (data == null) {
    data=behavior;
  }
 else {
    if (data instanceof Object[]) {
      Object[] tmp=(Object[])data;
      Object[] array=new Object[tmp.length + 1];
      System.arraycopy(tmp,0,array,0,tmp.length);
      array[tmp.length]=behavior;
      data=array;
    }
 else {
      Object[] array=new Object[2];
      array[0]=data;
      array[1]=behavior;
      data=array;
    }
  }
}
