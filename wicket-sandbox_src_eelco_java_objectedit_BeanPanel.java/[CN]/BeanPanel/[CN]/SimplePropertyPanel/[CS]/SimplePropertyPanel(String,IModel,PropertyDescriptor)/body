{
  super(id,beanModel,descriptor);
  Class type=descriptor.getPropertyType();
  TextField valueTextField=new TextField("value",new PropertyDescriptorModel(beanModel,descriptor),type);
  EditModeReplacementModel replacementModel=new EditModeReplacementModel(editMode,descriptor);
  valueTextField.add(new AttributeModifier("disabled",false,replacementModel));
  add(valueTextField);
}
