{
  if (currentValue == null) {
    return null;
  }
  StringBuffer sb=new StringBuffer(currentValue);
  if (!Strings.isEmpty(currentValue) && !Strings.isEmpty(appendValue)) {
    sb.append(separator);
  }
  if (!Strings.isEmpty(appendValue)) {
    sb.append(appendValue);
  }
  return sb.toString();
}
