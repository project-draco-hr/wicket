{
  boolean accept=false;
  if (pathname.isDirectory()) {
    boolean found=false;
    for (int i=0; i < ignoreDirectory.length; i++) {
      String ignore=ignoreDirectory[i];
      if (pathname.getName().equals(ignore)) {
        found=true;
        break;
      }
    }
    if (found == false) {
      accept=true;
    }
  }
  return accept;
}
