{
  boolean hit=false;
  try {
    executeTest(SimplePage_8.class,"SimplePageExpectedResult_8.html");
  }
 catch (  WicketRuntimeException ex) {
    if ((ex.getCause() != null) && (ex.getCause() instanceof MarkupException)) {
      hit=true;
    }
  }
  assertTrue("Did expect a MarkupException",hit);
}
