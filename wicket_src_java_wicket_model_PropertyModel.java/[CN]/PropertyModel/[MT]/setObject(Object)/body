{
  try {
    String expr=getExpression();
    Object target=getModel().getObject();
    if (object != null && propertyType != null && (object instanceof String) && (!((String)object).trim().equals(""))) {
      object=converterProvider.getConverter().convert(object,propertyType);
    }
    OgnlContext ctx=getContext();
    Ognl.setValue(expr,ctx,target,object);
  }
 catch (  OgnlException e) {
    throw new WicketRuntimeException(e);
  }
}
