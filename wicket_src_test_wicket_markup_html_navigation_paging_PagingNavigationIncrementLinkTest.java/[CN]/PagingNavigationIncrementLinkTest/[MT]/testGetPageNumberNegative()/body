{
  PagingNavigationIncrementLink link=new PagingNavigationIncrementLink(new EmptyPage(),"",mock,-1);
  assertEquals(0,link.getPageNumber());
  pagecount=1;
  assertEquals(0,link.getPageNumber());
  pagecount=2;
  assertEquals(0,link.getPageNumber());
  currentpage=1;
  assertEquals(0,link.getPageNumber());
  pagecount=3;
  currentpage=2;
  assertEquals(1,link.getPageNumber());
}
