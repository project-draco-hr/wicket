{
  Class clz=object.getClass().getComponentType();
  Object value=null;
  try {
    value=clz.newInstance();
    Array.set(object,index,value);
  }
 catch (  Exception e) {
    log.warn("Cannot set new value " + value + " at index "+ index+ " for array holding elements of class "+ clz,e);
  }
  return value;
}
