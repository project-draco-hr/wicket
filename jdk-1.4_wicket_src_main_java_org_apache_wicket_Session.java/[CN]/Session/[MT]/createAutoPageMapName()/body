{
  String name=getAutoPageMapNamePrefix() + autoCreatePageMapCounter + getAutoPageMapNameSuffix();
  IPageMap pm=pageMapForName(name,false);
  while (pm != null) {
    autoCreatePageMapCounter++;
    name=getAutoPageMapNamePrefix() + autoCreatePageMapCounter + getAutoPageMapNameSuffix();
    pm=pageMapForName(name,false);
  }
  return name;
}
