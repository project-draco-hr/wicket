{
  dest.id=id;
  dest.flags=flags;
  dest.autoComponentFactory=autoComponentFactory;
  if (markupClassRef != null) {
    dest.setMarkupClass(markupClassRef.get());
  }
  if (behaviors != null) {
    dest.behaviors=new ArrayList<>(behaviors);
  }
  if (userData != null) {
    dest.userData=new HashMap<>(userData);
  }
}
