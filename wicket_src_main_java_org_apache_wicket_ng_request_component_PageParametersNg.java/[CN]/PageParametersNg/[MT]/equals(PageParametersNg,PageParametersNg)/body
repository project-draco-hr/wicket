{
  if (Objects.equal(p1,p2)) {
    return true;
  }
  if (p1 == null && p2.getIndexedParamsCount() == 0 && p2.getNamedParameterKeys().isEmpty()) {
    return true;
  }
  if (p2 == null && p1.getIndexedParamsCount() == 0 && p1.getNamedParameterKeys().isEmpty()) {
    return true;
  }
  return false;
}
