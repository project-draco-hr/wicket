{
  tester=new WicketTester(new WebApplication(){
    /** 
 * @see org.apache.wicket.protocol.http.WebApplication#newSession(Request,Response)
 */
    @Override public Session newSession(    Request request,    Response response){
      return new WebSession(request).setStyle("myStyle");
    }
    @Override public Class<? extends Page> getHomePage(){
      return ConcretePage2.class;
    }
    @Override protected WebResponse newWebResponse(    HttpServletResponse servletResponse){
      return new WebResponse(servletResponse);
    }
    @Override protected void outputDevelopmentModeWarning(){
    }
    @Override protected ISessionStore newSessionStore(){
      return new HttpSessionStore();
    }
  }
);
  executeTest(ConcretePage2.class,"ExpectedResult3.html");
}
