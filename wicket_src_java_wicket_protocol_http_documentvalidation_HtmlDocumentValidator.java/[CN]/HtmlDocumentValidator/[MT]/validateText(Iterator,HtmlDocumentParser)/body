{
  boolean valid=true;
  if (expectedElements.hasNext()) {
    DocumentElement e=(DocumentElement)expectedElements.next();
    if (e instanceof TextContent) {
      if (!parser.getText().matches(((TextContent)e).getValue())) {
        System.err.println("Found text '" + parser.getText() + "' does not match "+ "expected text '"+ ((TextContent)e).getValue()+ "'");
        valid=false;
      }
    }
 else {
      System.err.println("Found text '" + parser.getText() + "' was not expected. "+ "We were expecting: "+ e.toString());
      valid=false;
    }
  }
 else {
    System.err.println("Found text '" + parser.getText() + "' was not expected. "+ "We were not expecting any more elements within the current tag");
    valid=false;
  }
  return valid;
}
