{
  if (dfos.isInMemory()) {
    if (cachedContent == null) {
      cachedContent=dfos.getData();
    }
    return cachedContent;
  }
  byte[] fileData=new byte[(int)getSize()];
  FileInputStream fis=null;
  try {
    fis=new FileInputStream(dfos.getFile());
    fis.read(fileData);
  }
 catch (  IOException e) {
    fileData=null;
  }
 finally {
    IOUtils.closeQuietly(fis);
  }
  return fileData;
}
