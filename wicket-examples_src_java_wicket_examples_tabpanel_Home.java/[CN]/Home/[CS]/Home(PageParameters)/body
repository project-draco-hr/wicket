{
  add(new NavigationPanel("mainNavigation","TabPanel example"));
  final TabPanel tab=new TabPanel("tabPanel");
  add(tab);
  tab.addTab("tab 1",new Label("tab","1111"));
  tab.addTab("tab 2",new Label("tab","2222"));
  tab.addTab("tab 3",new Label("tab","3333"));
  tab.addTab("tab 4",new Label("tab","4444"));
}
