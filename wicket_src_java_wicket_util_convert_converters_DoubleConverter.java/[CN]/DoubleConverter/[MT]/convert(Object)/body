{
  if (value == null) {
    return null;
  }
 else   if (value instanceof Double) {
    return (value);
  }
 else   if (value instanceof Number) {
    return new Double(((Number)value).doubleValue());
  }
 else {
    try {
      return (new Double(value.toString()));
    }
 catch (    Exception e) {
      throw new ConversionException(e);
    }
  }
}
