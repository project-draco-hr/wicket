{
  super();
  List list=new ArrayList();
  list.add("one");
  list.add("two");
  list.add("three");
  add(new ListView("table",list){
    protected void populateItem(    ListItem listItem){
      String txt=(String)listItem.getModelObject();
      listItem.add(new Label("txt",txt));
    }
  }
);
}
