{
  if (tag instanceof WicketTag) {
    WicketTag wtag=(WicketTag)tag;
    if (wtag.isLinkTag() && (wtag.getNamespace() != null)) {
      String id=tag.getId() + "-" + container.getPage().getAutoIndex();
      tag.setId(id);
      return new TransparentWebMarkupContainer(id);
    }
  }
  return null;
}
