{
  final ComponentTag tag=nextComponentTag();
  if (tag == null || tag.getId() != null) {
    return tag;
  }
  if (tag.getId() != null) {
    return tag;
  }
  if (contextPath != null) {
    for (    final String attrName : attributeNames) {
      final String attrValue=tag.getAttributes().getString(attrName);
      if ((attrValue != null) && (attrValue.startsWith("/") == false) && (attrValue.indexOf(":") < 0)&& !(attrValue.startsWith("#"))) {
        final String url;
        if (this.contextPath != null) {
          url=contextPath + attrValue;
        }
 else {
          url=attrValue;
        }
        tag.getAttributes().put(attrName,url);
        tag.setModified(true);
      }
    }
  }
  return tag;
}
