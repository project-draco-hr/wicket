{
  try {
    Blob blob=getBlob();
    if (blob != null) {
      InputStream in=blob.getBinaryStream();
      ByteArrayOutputStream out=new ByteArrayOutputStream();
      Streams.copy(in,out);
      return out.toByteArray();
    }
    return new byte[]{};
  }
 catch (  SQLException e) {
    throw new RuntimeException(e);
  }
catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
