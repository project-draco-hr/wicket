{
  Settings settings=new Settings(tester.getApplication());
  List loaders=settings.getStringResourceLoaders();
  Assert.assertEquals("There should be 3 default loaders",3,loaders.size());
  Assert.assertTrue("First loader one should be the component one",loaders.get(0) instanceof ComponentStringResourceLoader);
  Assert.assertTrue("Second loader should be the application one",loaders.get(1) instanceof PackageStringResourceLoader);
  Assert.assertTrue("Third loader should be the application one",loaders.get(2) instanceof ClassStringResourceLoader);
}
