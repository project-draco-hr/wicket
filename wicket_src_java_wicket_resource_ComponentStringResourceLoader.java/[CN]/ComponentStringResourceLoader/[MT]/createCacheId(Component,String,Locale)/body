{
  final StringBuffer buffer=new StringBuffer();
  buffer.append(component.getClass().getName());
  if (style != null) {
    buffer.append('.');
    buffer.append(style);
  }
  if (locale != null) {
    buffer.append('.');
    buffer.append(locale.toString());
  }
  final String id=buffer.toString();
  return id;
}
