{
  StringBuffer buf=new StringBuffer(getClass().getName()).append("@").append(hashCode()).append(getPage().toString()).append("->").append(getTarget().getId()).append("->").append(getListenerMethod().getDeclaringClass()).append(".").append(getListenerMethod().getName());
  if (requestParameters != null) {
    buf.append(" (request paramaters: ").append(requestParameters.toString()).append(")");
  }
  return buf.toString();
}
