{
  ListView<Integer> lv=createListView(0);
  assertEquals(0,lv.getStartIndex());
  assertEquals(0,lv.getViewSize());
  lv=new ListView<Integer>(new MockPageWithOneComponent(),"component",new Model<List<Integer>>(null)){
    private static final long serialVersionUID=1L;
    @Override protected void populateItem(    final ListItem<Integer> listItem){
    }
  }
;
  assertEquals(0,lv.getStartIndex());
  assertEquals(0,lv.getViewSize());
  lv.setStartIndex(5);
  lv.setViewSize(10);
  assertEquals(0,lv.getStartIndex());
  assertEquals(10,lv.getViewSize());
}
