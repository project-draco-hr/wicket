{
  ListView lv=createListView(0);
  assertEquals(0,lv.getStartIndex());
  assertEquals(0,lv.getViewSize());
  lv=new ListView("listView",new Model(null)){
    protected void populateItem(    final ListItem listItem){
      ;
    }
  }
;
