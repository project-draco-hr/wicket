{
  if (getSelectedTab() != index) {
    if (index < 0 || index >= tabs.size()) {
      throw new IndexOutOfBoundsException();
    }
    setModelObject(new Integer(index));
    Panel panel=((ITab)tabs.get(index)).getPanel(TAB_PANEL_ID);
    if (get(TAB_PANEL_ID) == null) {
      add(panel);
    }
 else {
      replace(panel);
    }
  }
}
