{
  if (css) {
    if (Strings.isEmpty(string)) {
      resp.renderCSSReference(ref);
    }
 else {
      resp.renderCSSReference(ref,string);
    }
  }
 else {
    if (Strings.isEmpty(string)) {
      resp.renderJavaScriptReference(ref);
    }
 else {
      resp.renderJavaScriptReference(ref,string);
    }
  }
}
