{
  if (url == null) {
    return false;
  }
  List<String> urlSegments=url.getSegments();
  for (int i=0; i < segments.length; ++i) {
    if (!segments[i].equals(safeSegmentGetter(urlSegments,i,"")) && (getPlaceholder(segments[i]) == null && getOptionalPlaceholder(segments[i]) == null)) {
      return false;
    }
  }
  return true;
}
