{
  CharSequence javascript=js;
  if (needsEncoding(js)) {
    javascript=encode(js);
  }
  response.write("<");
  response.write(invocation);
  response.write(">");
  response.write("<![CDATA[");
  response.write(javascript);
  response.write("]]>");
  response.write("</");
  response.write(invocation);
  response.write(">");
  encodingBodyResponse.reset();
}
