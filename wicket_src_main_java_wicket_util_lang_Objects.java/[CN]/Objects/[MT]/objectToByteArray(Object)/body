{
  try {
    final ByteArrayOutputStream out=new ByteArrayOutputStream();
    ObjectOutputStream oos=null;
    try {
      oos=new ObjectOutputStream(out);
      oos.writeObject(object);
    }
  finally {
      if (oos != null) {
        oos.close();
      }
    }
    return out.toByteArray();
  }
 catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
}
