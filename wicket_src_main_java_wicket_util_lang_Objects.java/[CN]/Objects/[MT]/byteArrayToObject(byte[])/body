{
  try {
    final ByteArrayInputStream in=new ByteArrayInputStream(data);
    try {
      return new ObjectInputStream(in).readObject();
    }
  finally {
      in.close();
    }
  }
 catch (  ClassNotFoundException e) {
    e.printStackTrace();
    return null;
  }
catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
}
