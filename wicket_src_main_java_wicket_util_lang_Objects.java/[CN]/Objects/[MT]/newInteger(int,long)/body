{
switch (type) {
case BOOL:
case CHAR:
case INT:
    return new Integer((int)value);
case FLOAT:
  if (value == value) {
    return new Float(value);
  }
case DOUBLE:
if (value == value) {
  return new Double(value);
}
case LONG:
return new Long(value);
case BYTE:
return new Byte((byte)value);
case SHORT:
return new Short((short)value);
default :
return BigInteger.valueOf(value);
}
}
