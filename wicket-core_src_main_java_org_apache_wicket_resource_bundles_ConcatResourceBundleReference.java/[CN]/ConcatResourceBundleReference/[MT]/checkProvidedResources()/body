{
  for (  T curProvidedResource : providedResources) {
    ResourceReference reference=curProvidedResource.getReference();
    if (!(reference instanceof CssResourceReference || reference instanceof JavaScriptResourceReference)) {
      throw new IllegalArgumentException("ConcatResourceBundleReference only works with CssResourceReference and JavaScriptResourceReference, " + curProvidedResource + " provides a "+ Classes.simpleName(reference.getClass()));
    }
  }
}
