{
  for (  T curProvidedResource : providedResources) {
    if (!(curProvidedResource.getReference() instanceof PackageResourceReference)) {
      throw new IllegalArgumentException("ConcatResourceBundleReference only works with PackageResourceReference, " + curProvidedResource + " provides a "+ curProvidedResource.getReference().getClass().getSimpleName());
    }
  }
}
