{
  String licenseType=null;
  String header=extractLicenseHeader(file,0,20);
  if (header.indexOf("Apache License, Version 2.0") != -1) {
    licenseType="ASL2";
  }
 else   if (header.indexOf("The Apache Software License, Version 1.1") != -1) {
    licenseType="ASL1.1";
  }
  return licenseType;
}
