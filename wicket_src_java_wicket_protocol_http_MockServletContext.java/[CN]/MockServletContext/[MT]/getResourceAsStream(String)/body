{
  if (webappRoot == null)   return null;
  if (name.startsWith("/"))   name=name.substring(1);
  File f=new File(webappRoot,name);
  if (!f.exists())   return null;
 else {
    try {
      return new FileInputStream(f);
    }
 catch (    FileNotFoundException e) {
      e.printStackTrace();
      return null;
    }
  }
}
