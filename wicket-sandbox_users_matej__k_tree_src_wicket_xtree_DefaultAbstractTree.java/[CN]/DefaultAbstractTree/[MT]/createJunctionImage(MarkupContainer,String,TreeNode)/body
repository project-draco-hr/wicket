{
  if (!node.isLeaf()) {
    return new Image(parent,id){
      private static final long serialVersionUID=1L;
      @Override protected ResourceReference getImageResourceReference(){
        if (isNodeExpanded(node)) {
          return MINUS;
        }
 else {
          return PLUS;
        }
      }
    }
;
  }
 else {
    return new Image(parent,id,getBlank());
  }
}
