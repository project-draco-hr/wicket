{
  int n=t.size();
  if (n == 0)   return;
  for (; ; ) {
    Entry[] tab;
    int max;
synchronized (segments[0]) {
      tab=table;
      max=threshold * CONCURRENCY_LEVEL;
    }
    if (n < max)     break;
    resize(0,tab);
  }
  for (Iterator it=t.entrySet().iterator(); it.hasNext(); ) {
    Map.Entry entry=(Map.Entry)it.next();
    put(entry.getKey(),entry.getValue());
  }
}
