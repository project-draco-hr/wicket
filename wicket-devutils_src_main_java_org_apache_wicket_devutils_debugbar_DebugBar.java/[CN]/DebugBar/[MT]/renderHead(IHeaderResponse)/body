{
  response.render(CssHeaderItem.forReference(new CssResourceReference(DebugBar.class,"wicket-debugbar.css")));
  response.render(JavaScriptHeaderItem.forReference(new JavaScriptResourceReference(DebugBar.class,"wicket-debugbar.js")));
  response.render(OnDomReadyHeaderItem.forScript("wicketDebugBarCheckState()"));
}
