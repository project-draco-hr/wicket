{
  if (isMutable) {
    return this;
  }
 else {
    final ComponentTag tag=new ComponentTag();
    tag.namespace=namespace;
    tag.name=name;
    tag.pos=pos;
    tag.length=length;
    tag.text=text;
    tag.attributes=new ValueMap(attributes);
    tag.type=type;
    tag.isMutable=true;
    tag.componentName=componentName;
    tag.closes=closes;
    tag.copyOf=copyOf;
    return tag;
  }
}
