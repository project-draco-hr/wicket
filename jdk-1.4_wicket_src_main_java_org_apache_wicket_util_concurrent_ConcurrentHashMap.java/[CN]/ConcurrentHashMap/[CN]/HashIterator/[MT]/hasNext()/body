{
  for (; ; ) {
    if (entry != null) {
      Object v=entry.value;
      if (v != null) {
        currentKey=entry.key;
        currentValue=v;
        return true;
      }
 else {
        entry=entry.next;
      }
    }
    while (entry == null && index >= 0) {
      entry=tab[index--];
    }
    if (entry == null) {
      currentKey=currentValue=null;
      return false;
    }
  }
}
