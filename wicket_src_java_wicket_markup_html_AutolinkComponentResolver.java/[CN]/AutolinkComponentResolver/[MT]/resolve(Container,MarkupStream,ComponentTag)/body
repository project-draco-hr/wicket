{
  if (tag.isAutolinkEnabled()) {
    final String componentName=tag.getComponentName();
    final Component link=resolveAutomaticLink(container,componentName,tag);
    container.add(link);
    if (log.isDebugEnabled()) {
      log.debug("Added autolink " + link);
    }
    link.render();
    return true;
  }
  return false;
}
