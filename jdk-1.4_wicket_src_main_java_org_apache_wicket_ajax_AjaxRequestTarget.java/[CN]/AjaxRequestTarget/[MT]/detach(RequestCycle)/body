{
  if (header != null) {
    Page page=header.getPage();
    if (oldHeader == null) {
      page.remove(header);
    }
 else {
      page.replace(oldHeader);
    }
  }
  if (markupIdToComponent.size() > 0) {
    final Component component=(Component)markupIdToComponent.values().iterator().next();
    component.getPage().detach();
  }
}
