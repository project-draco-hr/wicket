{
  RequestCycle rc=RequestCycle.get();
  Session session=Session.get();
  session.bind();
  String key=session.getMetaData(KEY);
  if (key == null) {
    key=session.getId() + "." + UUID.randomUUID().toString();
    session.setMetaData(KEY,key);
  }
  ICrypt crypt=new SunJceCrypt();
  crypt.setKey(key);
  return crypt;
}
