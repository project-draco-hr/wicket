{
  if (path == null) {
    return (IRequestTargetUrlCodingStrategy)mountsOnPath.strategyForMount(null);
  }
 else   if (!path.equals("/")) {
    IRequestTargetUrlCodingStrategy strategy=mountsOnPath.strategyForPath(path);
    if (strategy != null) {
      return strategy;
    }
  }
  return null;
}
