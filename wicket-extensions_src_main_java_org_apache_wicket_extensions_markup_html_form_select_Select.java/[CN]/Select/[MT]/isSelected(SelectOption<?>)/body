{
  if (hasRawInput()) {
    String[] paths=getInputAsArray();
    if (paths != null && paths.length > 0) {
      for (      String path : paths) {
        if (path.equals(option.getPath())) {
          return true;
        }
      }
      return false;
    }
  }
  return compareModels(getDefaultModelObject(),option.getDefaultModelObject());
}
