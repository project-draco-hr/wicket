{
  removeAll();
  List visiblePathsList=new ArrayList();
  Enumeration e=treeState.getVisiblePathsFromRoot();
  boolean skip=(!treeState.isRootVisible());
  while (e.hasMoreElements()) {
    TreePath path=(TreePath)e.nextElement();
    if (skip) {
      skip=false;
      continue;
    }
    visiblePathsList.add(path);
  }
  List nestedList=new ArrayList();
  buildList(visiblePathsList,0,0,nestedList);
  add(getTreeRowsPanel("tree",nestedList));
}
