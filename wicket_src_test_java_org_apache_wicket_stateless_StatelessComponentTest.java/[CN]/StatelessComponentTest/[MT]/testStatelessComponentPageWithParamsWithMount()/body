{
  PageParameters params=new PageParameters();
  params.put("testParam1","testValue1");
  params.put("testParam2","testValue2");
  tester.getApplication().mountBookmarkablePage("/stateless",StatelessComponentPageWithParams.class);
  executeTest(StatelessComponentPageWithParams.class,params,"StatelessComponentPageWithParams_mount_result.html");
  tester.setupRequestAndResponse();
  tester.getServletRequest().setURL("/WicketTester$DummyWebApplication/WicketTester$DummyWebApplication/stateless/testParam1/testValue1/testParam2/testValue2/wicket:interface/%3A0%3Alink%3A%3AILinkListener%3A%3A/");
  try {
    tester.processRequestCycle();
    fail("An exception should have been thrown for this request!");
  }
 catch (  Exception e) {
    assertEquals("wanted exception",e.getMessage());
  }
}
