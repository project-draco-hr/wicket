{
  PageParameters params=new PageParameters();
  params.set("testParam1","testValue1");
  params.set("testParam2","testValue2");
  tester.getApplication().mountPage("/stateless",StatelessComponentPageWithParams.class);
  executeTest(StatelessComponentPageWithParams.class,params,"StatelessComponentPageWithParams_mount_result.html");
  tester.getRequest().setUrl(Url.parse("stateless?0-1.ILinkListener-link&amp;testParam1=testValue1&amp;testParam2=testValue2"));
  try {
    tester.processRequest();
    fail("An exception should have been thrown for this request!");
  }
 catch (  Exception e) {
    assertEquals("wanted exception",e.getMessage());
  }
}
