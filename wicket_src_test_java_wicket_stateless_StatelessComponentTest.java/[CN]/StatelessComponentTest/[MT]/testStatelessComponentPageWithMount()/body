{
  application.getApplication().mountBookmarkablePage("/stateless",StatelessComponentPage.class);
  application.setupRequestAndResponse();
  application.getServletRequest().setURL("/stateless/wicket:interface/:0:link::ILinkListener:");
  try {
    application.processRequestCycle();
    fail("An exception should have been thrown for this request!");
  }
 catch (  Exception e) {
    assertEquals("wanted exception",e.getMessage());
  }
}
