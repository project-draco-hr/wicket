{
  if (component == null) {
    throw new IllegalArgumentException("argument component may not be null");
  }
  component.setAuto(true);
  add(component);
  component.prepareForRender();
  if (markupStream == null) {
    component.render();
  }
 else {
    component.render(markupStream);
  }
  remove(component);
  return true;
}
