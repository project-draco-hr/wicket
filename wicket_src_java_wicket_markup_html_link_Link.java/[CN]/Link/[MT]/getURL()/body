{
  if (getPage().isStateless() == false) {
    return urlFor(ILinkListener.INTERFACE);
  }
 else {
    IRequestTarget requestTarget=new BookmarkableListenerInterfaceRequestTarget(getPage().getPageMap().getName(),getPage().getClass(),new PageParameters(),this,ILinkListener.INTERFACE);
    return urlFor(requestTarget);
  }
}
