{
  Application dummy=new DummyApplication();
  ApplicationSettings settings=new ApplicationSettings(dummy);
  settings.addStringResourceLoader(new BundleStringResourceLoader("wicket.resource.DummyResources"));
  settings.addStringResourceLoader(new ComponentStringResourceLoader(dummy));
  List loaders=settings.getStringResourceLoaders();
  Assert.assertEquals("There should be 2 overridden loaders",2,loaders.size());
  Assert.assertTrue("First loader one should be the bundle one",loaders.get(0) instanceof BundleStringResourceLoader);
  Assert.assertTrue("Second loader should be the component one",loaders.get(1) instanceof ComponentStringResourceLoader);
}
