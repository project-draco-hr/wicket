{
  if (separators == null || separators.length == 0) {
    return new HashMap<String,String>();
  }
  char separator=separators[0];
  if (str != null) {
    int idx=str.length();
    for (    char sep : separators) {
      int tmp=str.indexOf(sep);
      if (tmp != -1) {
        if (tmp < idx) {
          idx=tmp;
          separator=sep;
        }
      }
    }
  }
  return parse(str,separator);
}
