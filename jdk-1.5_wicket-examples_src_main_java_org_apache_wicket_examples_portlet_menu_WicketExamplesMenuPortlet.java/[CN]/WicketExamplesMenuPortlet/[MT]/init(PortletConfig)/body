{
  super.init(config);
  if (examples == null) {
    examples=discoverExamples(config.getPortletContext());
    if (examples == null) {
      examples=Collections.EMPTY_LIST;
    }
 else {
      examples=Collections.unmodifiableList(examples);
    }
    config.getPortletContext().setAttribute(EXAMPLES,examples);
  }
}
