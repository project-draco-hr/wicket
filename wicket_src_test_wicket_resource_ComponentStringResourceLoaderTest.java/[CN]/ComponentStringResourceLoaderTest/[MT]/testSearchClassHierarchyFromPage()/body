{
  DummySubClassPage p=new DummySubClassPage();
  IStringResourceLoader loader=new ComponentStringResourceLoader(new DummyApplication());
  Assert.assertEquals("Valid resource string should be found","SubClass Test String",loader.loadStringResource(p,"subclass.test.string",Locale.getDefault(),null));
  Assert.assertEquals("Valid resource string should be found","Another string",loader.loadStringResource(p,"another.test.string",Locale.getDefault(),null));
}
