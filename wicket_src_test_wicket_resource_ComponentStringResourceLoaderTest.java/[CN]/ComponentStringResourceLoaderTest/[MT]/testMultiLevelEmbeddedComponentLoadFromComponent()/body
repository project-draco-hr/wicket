{
  DummyPage p=new DummyPage();
  Panel panel=new Panel("panel");
  p.add(panel);
  DummyComponent c=new DummyComponent("hello",application);
  panel.add(c);
  IStringResourceLoader loader=new ComponentStringResourceLoader(new DummyApplication());
  Assert.assertEquals("Valid resourse string should be found","Component string",loader.loadStringResource(c,"component.string",Locale.getDefault(),null));
}
