{
  try {
    final Markup amarkup=new MarkupParser(new XmlPullParser()).parse(a);
    final Markup bmarkup=new MarkupParser(new XmlPullParser()).parse(b);
    return amarkup.equalTo(bmarkup);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
catch (  ResourceStreamNotFoundException e) {
    e.printStackTrace();
  }
  return false;
}
