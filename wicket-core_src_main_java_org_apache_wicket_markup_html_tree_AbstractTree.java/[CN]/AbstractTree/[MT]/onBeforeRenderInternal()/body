{
  if (attached == false) {
    onBeforeAttach();
    checkModel();
    if (dirtyAll && rootItem != null) {
      clearAllItem();
    }
 else {
      rebuildDirty();
    }
    if (rootItem == null) {
      Object rootNode=getModelObject().getRoot();
      if (rootNode != null) {
        if (isRootLess()) {
          rootItem=newTreeItem(null,rootNode,-1);
        }
 else {
          rootItem=newTreeItem(null,rootNode,0);
        }
        itemContainer.add(rootItem);
        buildItemChildren(rootItem);
      }
    }
    attached=true;
  }
}
