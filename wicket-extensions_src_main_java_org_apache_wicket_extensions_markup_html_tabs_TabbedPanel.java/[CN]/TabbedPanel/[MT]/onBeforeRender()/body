{
  if (!hasBeenRendered() && getSelectedTab() == -1) {
    List<ITab> tabs=getTabs();
    for (int i=0; i < tabs.size(); ++i) {
      ITab tab=tabs.get(i);
      if (tab.isVisible()) {
        setSelectedTab(i);
        break;
      }
    }
    if (tabs.size() == 0) {
      add(new WebMarkupContainer<Void>(TAB_PANEL_ID)).setVisible(false);
    }
  }
  super.onBeforeRender();
}
