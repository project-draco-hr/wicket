{
  int index=getSelectedTab();
  if ((index == -1) || (isTabVisible(index) == false)) {
    index=-1;
    for (int i=0; i < tabs.size(); i++) {
      if (isTabVisible(i)) {
        index=i;
        break;
      }
    }
    if (index != -1) {
      setSelectedTab(index);
    }
  }
  setCurrentTab(index);
  super.onBeforeRender();
}
