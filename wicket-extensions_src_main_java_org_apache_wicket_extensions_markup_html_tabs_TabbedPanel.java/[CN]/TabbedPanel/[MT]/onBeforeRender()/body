{
  if (getSelectedTab() == -1 || isTabVisible(getSelectedTab()) == false) {
    int selected=0;
    for (int i=0; i < tabs.size(); i++) {
      if (isTabVisible(i)) {
        selected=i;
        break;
      }
    }
    if (selected == tabs.size()) {
      selected=0;
    }
    setSelectedTab(selected);
  }
  super.onBeforeRender();
}
