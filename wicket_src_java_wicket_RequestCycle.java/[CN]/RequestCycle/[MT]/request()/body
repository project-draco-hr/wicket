{
  internalOnBeginRequest();
  onBeginRequest();
synchronized (session) {
    threadAttach();
    try {
      if (onRespond()) {
        final Page page=getResponsePage();
        if (page != null) {
          try {
            if (getRedirect()) {
              redirectToPage(page);
            }
 else {
              page.render();
            }
          }
  finally {
            internalOnEndRequest();
            onEndRequest();
            page.internalOnEndRequest();
            page.onEndRequest();
          }
        }
      }
 else {
        internalOnEndRequest();
        onEndRequest();
      }
    }
 catch (    RuntimeException e) {
      onRuntimeException(e);
    }
 finally {
      threadDetach();
    }
  }
}
