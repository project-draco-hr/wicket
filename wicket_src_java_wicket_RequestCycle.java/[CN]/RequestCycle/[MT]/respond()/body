{
synchronized (session) {
    threadAttach();
    try {
      if (onRespond()) {
        final Page page=getPage();
        if (page != null) {
          if (getRedirect()) {
            redirectToPage(page);
          }
 else {
            page.render();
          }
        }
      }
    }
 catch (    RuntimeException e) {
      onRuntimeException(e);
    }
 finally {
      threadDetach();
    }
  }
}
