{
  final Page page=getResponsePage();
  if (page != null) {
    try {
      if (getRedirect()) {
        redirectTo(page);
      }
 else {
        if (getInvokePage() == getResponsePage()) {
          setInvokePage(null);
        }
        page.doRender();
      }
    }
 catch (    RuntimeException e) {
      internalOnRuntimeException(page,e);
    }
  }
}
