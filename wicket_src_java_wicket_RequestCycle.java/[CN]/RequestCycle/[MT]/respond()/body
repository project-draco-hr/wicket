{
  beginResponse();
synchronized (session) {
    threadAttach();
    try {
      if (onRespond()) {
        final Page page=getPage();
        if (page != null) {
          try {
            if (getRedirect()) {
              redirectToPage(page);
            }
 else {
              page.render();
            }
          }
  finally {
            endResponse();
            page.onInternalEndRequest();
            page.onEndRequest();
          }
        }
      }
 else {
        endResponse();
      }
    }
 catch (    RuntimeException e) {
      onRuntimeException(e);
    }
 finally {
      threadDetach();
    }
  }
}
