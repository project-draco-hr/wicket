{
  try {
    IRequestCycleProcessor processor=safeGetRequestProcessor();
    while (currentStep < DONE) {
      try {
        step(processor);
        currentStep++;
      }
 catch (      AbstractRestartResponseException e) {
        currentStep=RESPOND;
      }
    }
  }
  finally {
    currentStep=CLEANUP_REQUEST;
    cleanUp();
    currentStep=DONE;
  }
}
