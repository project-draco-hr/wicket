{
  Object synchronizeLock=getSynchronizationLock();
  if (synchronizeLock != null) {
synchronized (synchronizeLock) {
      processor.processEvents(this);
      currentStep=RESPOND;
      processor.respond(this);
    }
  }
 else {
    processor.processEvents(this);
    currentStep=RESPOND;
    processor.respond(this);
  }
}
