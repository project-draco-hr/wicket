{
  if (pageMap == null) {
    pageMap=PageMap.forName(PageMap.DEFAULT_NAME);
  }
  IRequestTarget target=new BookmarkablePageRequestTarget(pageMap.getName(),pageClass,parameters);
  IRequestCodingStrategy requestCodingStrategy=getProcessor().getRequestCodingStrategy();
  return requestCodingStrategy.encode(this,target);
}
