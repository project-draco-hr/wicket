{
  setupOutput();
  if (label != null) {
    outfile.println(mark + ' ' + label);
  }
 else   if (inf.lastModified() > 0) {
    outfile.println(mark + ' ' + inf.getPath()+ '\t'+ new SimpleDateFormat("yyyy.MM.dd HH:mm:ss").format(new Date(inf.lastModified())));
  }
 else {
    outfile.println(mark + ' ' + inf.getPath());
  }
}
