{
  if (key == null) {
    return null;
  }
  Object val=parameterMap.get(key);
  if (val == null) {
    return null;
  }
 else   if (val instanceof String[]) {
    return (String[])val;
  }
 else   if (val instanceof String) {
    return new String[]{(String)val};
  }
 else {
    return new String[]{val.toString()};
  }
}
