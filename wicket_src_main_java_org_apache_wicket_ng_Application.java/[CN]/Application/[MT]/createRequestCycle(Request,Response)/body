{
  RequestCycleContext context=new RequestCycleContext(request,response,getRootRequestMapper(),new DefaultExceptionMapper());
  RequestCycle requestCycle=newRequestCycle(context);
  requestCycle.register(new RequestCycle.DetachCallback(){
    public void onDetach(    RequestCycle requestCycle){
      getPageManager().commitRequest();
    }
  }
);
  return requestCycle;
}
