{
  boolean guessed=attributes.getParameters().getNamedParameter(PARAMETER_GUESSED).toBoolean(false);
  String letter=attributes.getParameters().getNamedParameter(PARAMETER_LETTER).toString();
  DefaultButtonImageResource buttonResource=new DefaultButtonImageResource(30,30,letter);
  if (guessed) {
    buttonResource.setColor(Color.GRAY);
  }
  buttonResource.respond(attributes);
}
