{
  super.renderHead(container);
  if (isRenderChildren()) {
    visitItemChildren(this,new IItemCallback(){
      @Override public void visitItem(      TreeItem item){
        item.visitChildren(new IVisitor<Component,Void>(){
          @Override public void component(          final Component component,          final IVisit<Void> visit){
            if (component.isVisible()) {
              component.renderHead(container);
            }
 else {
              visit.dontGoDeeper();
            }
          }
        }
);
      }
    }
);
  }
}
