{
  ConcatBundleResource bundle=new ConcatBundleResource(Arrays.asList(JavaScriptHeaderItem.forReference(new JavaScriptResourceReference(ResouceBundleTest.class,"a.js")),JavaScriptHeaderItem.forReference(new JavaScriptResourceReference(ResouceBundleTest.class,"b.js"))));
  tester.startResource(bundle);
  assertEquals("//a\n//b\n",tester.getLastResponseAsString());
}
