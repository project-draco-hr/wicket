{
  if (selected != null) {
    for (final StringTokenizer tokenizer=new StringTokenizer(selected,VALUE_SEPARATOR); tokenizer.hasMoreTokens(); ) {
      final String id=tokenizer.nextToken();
      if (id.equals(getChoiceRenderer().getIdValue(choice,index))) {
        return true;
      }
    }
  }
  return false;
}
