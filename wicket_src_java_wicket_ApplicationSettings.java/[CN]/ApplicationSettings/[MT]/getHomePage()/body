{
  if (homePage != null) {
    return homePage;
  }
  if (homePageClassName != null && homePageClassName.trim().length() > 0) {
    final Class homePage=getDefaultPageFactory().classForName(homePageClassName);
    if (homePage != null) {
      return homePage;
    }
  }
  throw new IllegalStateException("No home page specified");
}
