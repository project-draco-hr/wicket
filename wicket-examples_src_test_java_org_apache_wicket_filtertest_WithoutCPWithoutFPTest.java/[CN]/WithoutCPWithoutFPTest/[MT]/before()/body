{
  if (getContextPath() == null) {
    setContextPath("");
  }
  if (getWebappLocation() == null) {
    String basedir=System.getProperty("basedir");
    String path="";
    if (basedir != null)     path=basedir + "/";
    path+="src/main/testwebapp2";
    setWebappLocation(path);
  }
  super.before();
}
