{
  int current=getTrailingSlashesCount(buffer);
  if (current > desiredCount) {
    buffer.setLength(buffer.length() - (current - desiredCount));
  }
 else   if (desiredCount > current) {
    int toAdd=desiredCount - current;
    while (toAdd > 0) {
      buffer.append("/");
      --toAdd;
    }
  }
}
