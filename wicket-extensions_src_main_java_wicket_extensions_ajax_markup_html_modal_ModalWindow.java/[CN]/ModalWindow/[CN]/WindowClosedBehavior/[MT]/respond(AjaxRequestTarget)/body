{
  shown=false;
  if (deletePageMap == true) {
    Session session=Session.get();
    PageMap pageMap=session.pageMapForName(getPageMapName(),false);
    if (pageMap != null) {
      session.removePageMap(pageMap);
      deletePageMap=false;
    }
  }
  if (windowClosedCallback != null) {
    windowClosedCallback.onClose(target);
  }
}
