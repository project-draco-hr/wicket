{
  final int buttons=countButtons();
  if (buttons <= 1) {
    validate();
  }
 else   if (buttons > 1) {
    final Button button=findSubmittingButton();
    if (button == null) {
      throw new WicketRuntimeException("Unable to find submitting button");
    }
 else {
      button.onSubmit();
    }
  }
}
