{
  if (bookmarkableHint) {
    try {
      Page page=getPage();
      if (page.getClass().getConstructor((Class[])null) != null || page.getClass().getConstructor(new Class[]{PageParameters.class}) != null) {
        return super.isStateless();
      }
    }
 catch (    Exception e) {
    }
  }
  return false;
}
