{
  final RequestParameters requestParameters=getRequest().getRequestParameters();
  final Component component=page.get(requestParameters.getComponentPath());
  if (component == null) {
    throw new WicketRuntimeException("Component not found: " + requestParameters.getComponentPath());
  }
  if (!component.isVisible()) {
    throw new WicketRuntimeException("Calling listener methods on components that are not visible is not allowed");
  }
  final RequestListenerInterface listener=requestParameters.getInterface();
  if (listener != null) {
    new FormSubmitInterfaceRequestTarget(page,component,listener).processEvents(getRequestCycle());
  }
}
