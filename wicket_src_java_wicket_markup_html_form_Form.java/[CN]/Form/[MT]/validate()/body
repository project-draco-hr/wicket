{
  int responseType=getApplicationSettings().getResponseType();
  setRedirect(responseType == 1 || responseType == 3);
  getValidationStrategy().validate(this);
  if (hasError()) {
    invalid();
    onError();
    return false;
  }
 else {
    persistFormComponentData();
    updateFormComponentModels();
    onSubmit();
    return true;
  }
}
