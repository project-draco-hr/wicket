{
  IMarkupFragment markup=container.getMarkup();
  if (markup == null) {
    return null;
  }
  if (child == null) {
    return markup;
  }
  markup=markup.find(child.getId());
  if (markup != null) {
    return markup;
  }
  if (!(child instanceof IComponentResolver)) {
    markup=searchMarkupInTransparentResolvers(container,child);
  }
  return markup;
}
