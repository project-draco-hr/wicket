{
  if (!(o instanceof Entry)) {
    return null;
  }
  Entry entry=(Entry)o;
  int key=entry.getKey();
  int i=indexFor(key,table.length);
  Entry prev=table[i];
  Entry e=prev;
  while (e != null) {
    Entry next=e.next;
    if (e.key == key && e.equals(entry)) {
      modCount++;
      size--;
      if (prev == e) {
        table[i]=next;
      }
 else {
        prev.next=next;
      }
      return e;
    }
    prev=e;
    e=next;
  }
  return e;
}
