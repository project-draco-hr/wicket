{
  if (array != null) {
    String[] copy=new String[array.length];
    for (int i=0; i < array.length; i++) {
      String el=array[i];
      if (el != null) {
        if (el.length() > len) {
          copy[i]=el.substring(0,len);
        }
 else {
          copy[i]=el;
        }
      }
    }
    return copy;
  }
  return null;
}
