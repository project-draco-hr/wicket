{
  if (key == null) {
    return null;
  }
  Object val=parameterMap.get(key);
  if (val == null) {
    return null;
  }
 else   if (val instanceof String[]) {
    String[] arrayVal=(String[])val;
    return arrayVal.length > 0 ? arrayVal[0] : null;
  }
 else   if (val instanceof String) {
    return (String)val;
  }
 else {
    return val.toString();
  }
}
