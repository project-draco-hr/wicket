{
  super(id);
  OrderByLink link=new OrderByLink("orderByLink",property,stateLocator,OrderByLink.VoidCssProvider.getInstance()){
    private static final long serialVersionUID=1L;
    @Override protected void onSortChanged(){
      OrderByBorder.this.onSortChanged();
    }
  }
;
  add(link);
