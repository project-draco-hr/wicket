{
  if (!node.isLeaf()) {
    return new Image(JUNCTION_IMAGE_NAME){
      protected ResourceReference getImageResourceReference(){
        if (isExpanded(node)) {
          return minus;
        }
 else {
          return plus;
        }
      }
    }
;
  }
 else {
    return new Image(JUNCTION_IMAGE_NAME,blank);
  }
}
