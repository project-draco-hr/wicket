{
  if (!node.isLeaf()) {
    return new Image(JUNCTION_IMAGE_NAME,(SharedResource)null){
      protected Resource getImageResource(){
        if (isExpanded(node)) {
          return getImage("minus.gif");
        }
 else {
          return getImage("plus.gif");
        }
      }
    }
;
  }
 else {
    return new Image(JUNCTION_IMAGE_NAME,getImage("blank.gif"));
  }
}
