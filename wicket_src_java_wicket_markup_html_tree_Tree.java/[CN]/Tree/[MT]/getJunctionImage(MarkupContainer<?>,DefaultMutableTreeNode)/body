{
  if (!node.isLeaf()) {
    return new Image(parent,JUNCTION_IMAGE_NAME){
      private static final long serialVersionUID=1L;
      @Override protected ResourceReference getImageResourceReference(){
        if (isExpanded(node)) {
          return MINUS;
        }
 else {
          return PLUS;
        }
      }
    }
;
  }
 else {
    return new Image(parent,JUNCTION_IMAGE_NAME,BLANK);
  }
}
