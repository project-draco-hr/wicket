{
  new FeedbackPanel(this,"feedback");
  Form form=new Form(this,"form");
  new SubmitLink(form,"internal"){
    @Override public void onSubmit(){
      info("internal onsubmit");
    }
  }
;
  new SubmitLink(this,"external",form){
    @Override public void onSubmit(){
      info("external onsubmit");
    }
  }
;
}
