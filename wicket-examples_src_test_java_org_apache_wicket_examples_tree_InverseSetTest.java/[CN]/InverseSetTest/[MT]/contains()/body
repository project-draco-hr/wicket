{
  InverseSet<String> inverse=new InverseSet<String>(set);
  assertFalse(inverse.contains("A"));
  assertTrue(inverse.contains("B"));
  inverse.remove("B");
  assertFalse(inverse.contains("A"));
  assertFalse(inverse.contains("B"));
  inverse.add("A");
  assertTrue(inverse.contains("A"));
  assertFalse(inverse.contains("B"));
  inverse.detach();
  assertTrue(set.detached);
}
