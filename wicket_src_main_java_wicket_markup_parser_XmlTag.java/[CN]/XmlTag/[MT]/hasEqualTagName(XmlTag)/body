{
  if (!getName().equalsIgnoreCase(tag.getName())) {
    return false;
  }
  if ((getNamespace() == null) && (tag.getNamespace() == null)) {
    return true;
  }
  if ((getNamespace() != null) && (tag.getNamespace() != null)) {
    return getNamespace().equalsIgnoreCase(tag.getNamespace());
  }
  return false;
}
