{
  if (!node.isLeaf()) {
    IModel imgModel=new Model(){
      public Object getObject(){
        final String url;
        if (isExpanded(node)) {
          url=getRequestCycle().urlFor(JUNCTION_IMG_MINUS,IResourceListener.class);
        }
 else {
          url=getRequestCycle().urlFor(JUNCTION_IMG_PLUS,IResourceListener.class);
        }
        return url;
      }
    }
;
    LocalImage img=new LocalImage(JUNCTION_IMAGE_NAME,imgModel);
    return img;
  }
 else {
    String url=getRequestCycle().urlFor(JUNCTION_IMG_BLANK,IResourceListener.class);
    LocalImage img=new LocalImage(JUNCTION_IMAGE_NAME,url);
    return img;
  }
}
