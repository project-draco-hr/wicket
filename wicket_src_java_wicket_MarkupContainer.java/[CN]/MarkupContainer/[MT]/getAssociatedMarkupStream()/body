{
  try {
    return getApplication().getMarkupCache().getMarkupStream(this,null);
  }
 catch (  WicketRuntimeException ex) {
    throw new WicketRuntimeException(exceptionMessage("Markup of type '" + getMarkupType() + "' for component '"+ getClass().getName()+ "' not found."+ " Enable debug messages for wicket.util.resource.Resource to get a list of all filenames tried"));
  }
}
