{
  List<ListObject> data=new TestList(10,false);
  new ListViewWithAlternatingRowStyle<ListObject>(this,"entries",data){
    /** 
 * populateItem() is called for each item of the list. ListItem
 * provides, beside the IModel, the current index while iterating
 * over the data provided.
 */
    @Override public void populateItem(    final ListItem listItem){
      final ListObject value=(ListObject)listItem.getModelObject();
      new Label(listItem,"entry",value.getName());
    }
  }
;
}
