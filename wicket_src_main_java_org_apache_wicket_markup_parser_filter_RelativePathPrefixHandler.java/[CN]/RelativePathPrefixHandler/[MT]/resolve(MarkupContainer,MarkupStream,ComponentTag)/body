{
  if ((tag != null) && (tag.getId().startsWith(WICKET_RELATIVE_PATH_PREFIX_CONTAINER_ID))) {
    final Component wc;
    String id=WICKET_RELATIVE_PATH_PREFIX_CONTAINER_ID + container.getPage().getAutoIndex();
    wc=new WebMarkupContainer(id){
      private static final long serialVersionUID=1L;
      /** 
 * @see org.apache.wicket.Component#getMarkup()
 */
      @Override public IMarkupFragment getMarkup(){
        return getParent().getMarkup(new WebComponent(WICKET_RELATIVE_PATH_PREFIX_CONTAINER_ID));
      }
      @Override public boolean isTransparentResolver(){
        return tag.isOpenClose() == false;
      }
    }
;
    container.autoAdd(wc,markupStream);
    return true;
  }
  return false;
}
