{
  if ((tag != null) && (tag.getId().startsWith(WICKET_RELATIVE_PATH_PREFIX_CONTAINER_ID))) {
    final Component wc;
    String id=WICKET_RELATIVE_PATH_PREFIX_CONTAINER_ID + container.getPage().getAutoIndex();
    tag.setId(id);
    if (tag.isOpenClose()) {
      wc=new WebComponent(id);
    }
 else {
      wc=new WebMarkupContainer(id){
        private static final long serialVersionUID=1L;
        @Override public boolean isTransparentResolver(){
          return true;
        }
      }
;
    }
    container.autoAdd(wc,markupStream);
    return true;
  }
  return false;
}
