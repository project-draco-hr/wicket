{
  if (location.startsWith("../")) {
    String url=getURL();
    int index=url.lastIndexOf("/");
    if (index != -1) {
      url=url.substring(0,index + 1) + location;
      url=RequestUtils.removeDoubleDots(url);
      index=url.indexOf("/");
      location=url.substring(index + 1);
    }
  }
  redirectLocation=location;
}
