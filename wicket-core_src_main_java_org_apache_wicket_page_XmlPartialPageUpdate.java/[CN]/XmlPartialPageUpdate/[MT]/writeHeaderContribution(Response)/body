{
  if (headerBuffer.getContents().length() != 0) {
    response.write("<header-contribution>");
    response.write("<![CDATA[<head xmlns:wicket=\"http://wicket.apache.org\">");
    response.write(encode(headerBuffer.getContents()));
    response.write("</head>]]>");
    response.write("</header-contribution>");
  }
}
