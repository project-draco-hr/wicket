{
  if (messages != null) {
    List sublist=new ArrayList();
    for (Iterator i=messages.iterator(); i.hasNext(); ) {
      UIMessage message=(UIMessage)i.next();
      if (message.isLevel(level)) {
        sublist.add(message);
      }
    }
    return new UIMessages().setMessages(sublist);
  }
 else {
    return new UIMessages();
  }
}
