{
  StringBuffer b=new StringBuffer("UIMessages{");
  List msgs=getMessages();
  if (!msgs.isEmpty()) {
    for (Iterator i=msgs.iterator(); i.hasNext(); ) {
      b.append(i.next());
      if (i.hasNext())       b.append(",");
    }
  }
 else {
    b.append("<empty>");
  }
  b.append("}");
  return b.toString();
}
