{
  AppendingStringBuffer sb=new AppendingStringBuffer(128);
  if (handler != null) {
    sb.append("handler=");
    sb.append(handler.getClass().isAnonymousClass() ? handler.getClass().getName() : handler.getClass().getSimpleName());
    if (handler instanceof ILoggableRequestHandler) {
      sb.append(",data=");
      sb.append(((ILoggableRequestHandler)handler).getLogData());
    }
  }
 else {
    sb.append("none");
  }
  return sb.toString();
}
