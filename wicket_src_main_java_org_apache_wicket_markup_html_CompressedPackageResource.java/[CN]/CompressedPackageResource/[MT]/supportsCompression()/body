{
  if (Application.get().getResourceSettings().getDisableGZipCompression()) {
    return false;
  }
  WebRequest request=(WebRequest)RequestCycle.get().getRequest();
  String s=request.getHeader("Accept-Encoding");
  if (s == null) {
    return false;
  }
 else {
    return s.indexOf("gzip") >= 0;
  }
}
