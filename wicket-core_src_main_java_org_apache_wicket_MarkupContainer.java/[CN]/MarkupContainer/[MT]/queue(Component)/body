{
  if (queue == null) {
    queue=new ComponentQueue();
  }
  queue.add(components);
  MarkupContainer region=null;
  Page page=null;
  MarkupContainer cursor=this;
  while (cursor != null) {
    if (region == null && cursor instanceof IQueueRegion) {
      region=cursor;
    }
    if (cursor instanceof Page) {
      page=(Page)cursor;
    }
    cursor=cursor.getParent();
  }
  if (page != null) {
    region.dequeue();
  }
  return this;
}
