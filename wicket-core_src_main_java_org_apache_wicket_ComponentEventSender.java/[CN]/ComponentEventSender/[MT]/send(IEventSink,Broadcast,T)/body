{
  ComponentEvent<?> event=new ComponentEvent<T>(sink,source,type,payload);
  Args.notNull(type,"type");
switch (type) {
case BUBBLE:
    bubble(event);
  break;
case BREADTH:
breadth(event);
break;
case DEPTH:
depth(event);
break;
case EXACT:
dispatcher.dispatchEvent(event.getSink(),event,((sink instanceof Component) ? (Component)sink : null));
break;
}
}
