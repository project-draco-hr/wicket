{
  final Folder folder=new Folder(path);
  if (folder.exists()) {
    log.debug("Added path '{}' as a folder.",path);
    super.add(folder);
  }
 else {
    if (!path.startsWith("/")) {
      path="/" + path;
    }
    if (!path.endsWith("/")) {
      path+="/";
    }
    log.debug("Added path '{}' as a web path.",path);
    webappPaths.add(path);
  }
}
