{
  if (signIn(getUsername(),getPassword())) {
    final RequestCycle cycle=getRequestCycle();
    if (cycle.continueToOriginalDestination()) {
      cycle.setPage(null);
    }
 else {
      cycle.setPage(getApplicationSettings().getDefaultPageFactory().newPage(getApplicationPages().getHomePage(),(PageParameters)null));
    }
  }
 else {
    error("Unable to sign you in");
  }
}
