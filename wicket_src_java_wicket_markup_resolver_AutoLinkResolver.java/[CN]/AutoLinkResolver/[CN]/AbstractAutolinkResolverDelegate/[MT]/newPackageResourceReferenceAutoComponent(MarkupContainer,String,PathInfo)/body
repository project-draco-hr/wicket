{
  if (!pathInfo.absolute) {
    Class clazz=((MarkupResourceStream)container.getMarkupStream().getResource()).getMarkupClass();
    PackageResourceReferenceAutolink autoLink=new PackageResourceReferenceAutolink(autoId,clazz,pathInfo.reference);
    if (autoLink.resourceReference != null) {
      return autoLink;
    }
 else {
      log.info("Did not find autolink resource: " + pathInfo.reference + "; Assume it is a valid external URL");
    }
  }
  return null;
}
