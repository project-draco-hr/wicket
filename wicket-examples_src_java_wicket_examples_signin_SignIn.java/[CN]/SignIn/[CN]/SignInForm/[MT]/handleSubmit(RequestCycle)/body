{
  if (properties.getString("username").equals("jonathan") && properties.getString("password").equals("password")) {
    cycle.getSession().setProperty("wicket.examples.signin.user","jonathan");
    if (!cycle.continueToOriginalDestination()) {
      cycle.setPage(new Home(PageParameters.NULL));
    }
  }
 else {
    handleError(new ValidationErrorMessage(this,"Couldn't sign you in"));
  }
}
