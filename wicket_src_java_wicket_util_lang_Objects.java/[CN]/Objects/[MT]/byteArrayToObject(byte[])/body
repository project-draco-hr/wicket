{
  try {
    final ByteArrayInputStream in=new ByteArrayInputStream(data);
    ObjectInputStream ois=null;
    try {
      ois=new ObjectInputStream(in);
      return ois.readObject();
    }
  finally {
      if (ois != null)       ois.close();
    }
  }
 catch (  ClassNotFoundException e) {
    e.printStackTrace();
    return null;
  }
catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
}
