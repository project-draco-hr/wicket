{
  if (YUILOADER == null) {
    StringBuffer sb=new StringBuffer();
    sb.append("yuiloader");
    if (Application.get().usesDeploymentConfig()) {
      sb.append("-min");
    }
    sb.append(".js");
    YUILOADER=new CompressedResourceReference(YuiLib.class,sb.toString());
  }
  return YUILOADER;
}
