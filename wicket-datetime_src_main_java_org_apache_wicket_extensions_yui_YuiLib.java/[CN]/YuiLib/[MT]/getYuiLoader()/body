{
  if (YUILOADER == null) {
    StringBuffer sb=new StringBuffer();
    sb.append("yuiloader");
    if (Application.DEPLOYMENT.equals(Application.get().getConfigurationType())) {
      sb.append("-min");
    }
    sb.append(".js");
    YUILOADER=new JavascriptResourceReference(YuiLib.class,sb.toString());
  }
  return YUILOADER;
}
