{
  final String pageClassName=request.getParameter("bookmarkablePage");
  if (pageClassName != null) {
    try {
      final Class pageClass=session.getClassResolver().resolveClass(pageClassName);
      Page newPage=session.getPageFactory().newPage(pageClass,new PageParameters(getRequest().getParameterMap()));
      setResponsePage(newPage);
      setUpdateCluster(true);
      newPage.internalBeginRequest();
      return true;
    }
 catch (    RuntimeException e) {
      throw new WicketRuntimeException("Unable to instantiate Page class: " + pageClassName + ". See below for details.",e);
    }
  }
  return false;
}
