{
  IMarkupFragment markup=new MyPage().getAssociatedMarkup();
  compareMarkupWithFile(markup,"MyPage_ExpectedResult.html",MyPage.class);
  markup=new MyPage().getMarkup();
  compareMarkupWithFile(markup,"MyPage_ExpectedResult.html",MyPage.class);
  markup=new MyPage().getMarkup(null);
  compareMarkupWithFile(markup,"MyPage_ExpectedResult.html",MyPage.class);
}
