{
  tester.getSession().setLocale(Locale.ENGLISH);
  StringResourceModel model=new StringResourceModel("with.quote.and.no.substitution",page,null,(Object[])null);
  assertEquals("Let's play in the rain!",model.getString());
  model=new StringResourceModel("with.quote.substitution",page,null,new Object[]{"rain!"});
  assertEquals("Let's play in the rain!",model.getString());
}
