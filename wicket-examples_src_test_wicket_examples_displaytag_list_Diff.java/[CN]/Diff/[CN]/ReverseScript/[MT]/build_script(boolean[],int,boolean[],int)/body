{
  change script=null;
  int i0=0, i1=0;
  while (i0 < len0 || i1 < len1) {
    if (changed0[1 + i0] || changed1[1 + i1]) {
      int line0=i0, line1=i1;
      while (changed0[1 + i0])       ++i0;
      while (changed1[1 + i1])       ++i1;
      script=new change(line0,line1,i0 - line0,i1 - line1,script);
    }
    i0++;
    i1++;
  }
  return script;
}
