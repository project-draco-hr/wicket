{
  if (value == null) {
    return null;
  }
  if ("".equals(value)) {
    if (type == String.class) {
      return "";
    }
    return null;
  }
  try {
    return Objects.convertValue(value,type);
  }
 catch (  Exception e) {
    throw new ConversionException(e.getMessage(),e).setSourceValue(value);
  }
}
