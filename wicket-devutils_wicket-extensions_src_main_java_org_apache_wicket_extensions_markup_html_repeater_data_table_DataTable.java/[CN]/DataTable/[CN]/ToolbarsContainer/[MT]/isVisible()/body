{
  final boolean[] visible=new boolean[]{false};
  visitChildren(new IVisitor(){
    public Object component(    Component component){
      if (component.isVisible()) {
        visible[0]=true;
        return STOP_TRAVERSAL;
      }
 else {
        return CONTINUE_TRAVERSAL_BUT_DONT_GO_DEEPER;
      }
    }
  }
);
  return visible[0];
}
