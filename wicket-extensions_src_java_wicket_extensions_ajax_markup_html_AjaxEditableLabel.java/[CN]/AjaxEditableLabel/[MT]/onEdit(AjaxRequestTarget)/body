{
  label.setVisible(false);
  editor.setVisible(true);
  target.addComponent(AjaxEditableLabel.this);
  target.appendJavascript("{ var el=wicketGet('" + editor.getMarkupId() + "');"+ "  el.focus(); "+ "  if (el.createTextRange) { "+ "     var v = el.value; var r = el.createTextRange(); "+ "     r.moveStart('character', v.length); r.select(); } }");
}
