{
  Iterator<T> it=getRecorderComponent().getSelectedChoices();
  modelChanging();
  Collection<T> collection=getModelCollection();
  collection.clear();
  while (it.hasNext()) {
    collection.add(it.next());
  }
  modelChanged();
  @SuppressWarnings("unchecked") IModel<Object> defaultModel=(IModel<Object>)getDefaultModel();
  defaultModel.setObject(collection);
}
