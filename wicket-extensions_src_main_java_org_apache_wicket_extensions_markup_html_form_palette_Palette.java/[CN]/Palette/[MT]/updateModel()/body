{
  modelChanging();
  Collection<T> model=getModelCollection();
  model.clear();
  Iterator<T> it=getRecorderComponent().getSelectedChoices();
  while (it.hasNext()) {
    final T selectedChoice=it.next();
    model.add(selectedChoice);
  }
  modelChanged();
  ((IModel<Object>)getDefaultModel()).setObject(model);
}
