{
  if (url == null) {
    return null;
  }
  int ixSemiColon=url.indexOf(";");
  if (ixSemiColon == -1) {
    return url;
  }
  int ixQuestionMark=url.indexOf("?");
  if (ixQuestionMark == -1) {
    return url.substring(0,ixSemiColon);
  }
  if (ixQuestionMark <= ixSemiColon) {
    return url;
  }
  return url.substring(0,ixSemiColon) + url.substring(ixQuestionMark);
}
