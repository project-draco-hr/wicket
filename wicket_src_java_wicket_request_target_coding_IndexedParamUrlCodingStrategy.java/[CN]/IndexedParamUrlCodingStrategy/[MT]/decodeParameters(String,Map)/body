{
  PageParameters params=new PageParameters();
  if (urlFragment == null) {
    return params;
  }
  if (urlFragment.startsWith("/")) {
    urlFragment=urlFragment.substring(1);
  }
  String[] parts=urlFragment.split("/");
  for (int i=0; i < parts.length; i++) {
    params.put(String.valueOf(i),parts[i]);
  }
  return params;
}
