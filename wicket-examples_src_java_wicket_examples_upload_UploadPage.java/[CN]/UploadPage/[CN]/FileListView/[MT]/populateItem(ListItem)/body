{
  final File file=(File)listItem.getModelObject();
  listItem.add(new Label(listItem,"file",file.getName()));
  listItem.add(new Link(listItem,"delete"){
    @Override public void onClick(){
      Files.remove(file);
      refreshFiles();
      UploadPage.this.info("Deleted " + file);
    }
  }
);
}
