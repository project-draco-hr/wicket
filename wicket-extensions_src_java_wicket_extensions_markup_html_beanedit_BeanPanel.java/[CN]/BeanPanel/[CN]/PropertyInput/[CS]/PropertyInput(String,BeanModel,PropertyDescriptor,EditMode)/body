{
  super(id,beanModel,descriptor,editMode);
  Class type=descriptor.getPropertyType();
  TextField valueTextField=new TextField("value",new BeanPropertyModel(beanModel,descriptor),type);
  EditModeReplacementModel replacementModel=new EditModeReplacementModel(editMode,descriptor);
  valueTextField.add(new AttributeModifier("disabled",true,replacementModel));
  add(valueTextField);
}
