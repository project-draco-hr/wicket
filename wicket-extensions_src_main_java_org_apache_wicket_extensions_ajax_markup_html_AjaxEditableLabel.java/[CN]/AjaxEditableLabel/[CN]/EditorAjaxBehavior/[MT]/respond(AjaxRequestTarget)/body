{
  RequestCycle requestCycle=RequestCycle.get();
  boolean save=Boolean.valueOf(requestCycle.getRequest().getParameter("save"));
  if (save) {
    editor.processInput();
    if (editor.isValid()) {
      onSubmit(target);
    }
 else {
      onError(target);
    }
  }
 else {
    onCancel(target);
  }
}
