{
  label.setVisible(false);
  editor.setVisible(true);
  target.add(AjaxEditableLabel.this);
  target.appendJavascript("{ var el=wicketGet('" + editor.getMarkupId() + "');"+ "   if (el.createTextRange) { "+ "     var v = el.value; var r = el.createTextRange(); "+ "     r.moveStart('character', v.length); r.select(); } }");
  target.focusComponent(editor);
}
