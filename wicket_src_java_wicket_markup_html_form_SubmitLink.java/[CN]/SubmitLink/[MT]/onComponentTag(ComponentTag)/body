{
  if (!isEnabled()) {
    if (tag.getName().equalsIgnoreCase("a")) {
      tag.setName("span");
      tag.remove("href");
    }
 else {
      tag.remove("onclick");
    }
  }
 else {
    if (tag.getName().equalsIgnoreCase("a")) {
      tag.put("href","#");
    }
    tag.put("onclick",getTriggerJavaScript());
  }
}
