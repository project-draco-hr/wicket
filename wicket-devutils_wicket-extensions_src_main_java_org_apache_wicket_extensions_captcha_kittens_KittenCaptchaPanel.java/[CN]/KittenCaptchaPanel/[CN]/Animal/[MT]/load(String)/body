{
  try {
    final BufferedImage loadedImage=ImageIO.read(new MemoryCacheImageInputStream(KittenCaptchaPanel.class.getResourceAsStream(filename + ".png")));
    final BufferedImage image=new BufferedImage(loadedImage.getWidth(),loadedImage.getHeight(),BufferedImage.TYPE_INT_ARGB);
    final Graphics2D graphics=image.createGraphics();
    graphics.drawImage(loadedImage,0,0,null);
    graphics.dispose();
    return image;
  }
 catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
}
