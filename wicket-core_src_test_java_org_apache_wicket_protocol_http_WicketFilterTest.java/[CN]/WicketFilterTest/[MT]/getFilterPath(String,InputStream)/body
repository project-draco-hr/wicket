{
  try {
    return new WebXmlFile().getUniqueFilterPath(false,filterName,in);
  }
 catch (  ParserConfigurationException ex) {
    throw new RuntimeException(ex);
  }
catch (  SAXException ex) {
    throw new RuntimeException(ex);
  }
catch (  IOException ex) {
    throw new RuntimeException(ex);
  }
}
