{
  PageKey oldPage=trackedPages.remove(trackingId);
  PageKey pageKey=new PageKey(page.getPageId(),Session.get().getId());
  if (oldPage != null && !oldPage.equals(pageKey))   subscriptions.removeAll(oldPage);
  trackedPages.forcePut(trackingId,pageKey);
  log.info("registered page {} for session {}",new Object[]{pageKey.getPageId(),pageKey.getSessionId()});
}
