{
  final Input input=new Input();
  setDefaultModel(new CompoundPropertyModel<Input>(input));
  FeedbackPanel feedbackPanel=new FeedbackPanel("feedback");
  add(feedbackPanel);
  Form<?> form=new Form("form"){
    @Override protected void onSubmit(){
      info("input: " + input);
    }
  }
;
  add(form);
  CheckBoxMultipleChoice<String> listChoice=new CheckBoxMultipleChoice<String>("sites",SITES);
  form.add(listChoice);
  listChoice=new CheckBoxMultipleChoice<String>("choices",MANY_CHOICES);
  form.add(listChoice);
}
