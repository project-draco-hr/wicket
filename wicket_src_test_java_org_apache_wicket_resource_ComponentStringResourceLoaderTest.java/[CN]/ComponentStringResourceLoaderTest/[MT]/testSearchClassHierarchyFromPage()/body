{
  DummySubClassPage p=new DummySubClassPage();
  IStringResourceLoader loader=new ComponentStringResourceLoader();
  Assert.assertEquals("Valid resource string should be found","SubClass Test String",loader.loadStringResource(p.getClass(),"subclass.test.string",Locale.getDefault(),null,null));
  Assert.assertEquals("Valid resource string should be found","Another string",loader.loadStringResource(p.getClass(),"another.test.string",Locale.getDefault(),null,null));
}
