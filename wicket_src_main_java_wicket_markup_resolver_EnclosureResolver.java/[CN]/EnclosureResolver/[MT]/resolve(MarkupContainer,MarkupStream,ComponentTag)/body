{
  if (tag.isEnclosureTag()) {
    String id=Component.AUTO_COMPONENT_PREFIX + "enclosure-" + container.getPage().getAutoIndex();
    final Enclosure enclosure=new Enclosure(container,id,tag.getString(EnclosureHandler.CHILD_ATTRIBUTE));
    enclosure.autoAdded();
    return true;
  }
  return false;
}
