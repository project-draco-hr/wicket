{
  if (selectedTab < 0) {
    for (int i=0; i < tabs.size(); i++) {
      final TabPanelModel model=(TabPanelModel)tabs.get(i);
      if (model.isEnabled()) {
        selectTab(i);
        break;
      }
    }
    if (selectedTab < 0) {
      return null;
    }
  }
  return (TabPanelModel)tabs.get(selectedTab);
}
