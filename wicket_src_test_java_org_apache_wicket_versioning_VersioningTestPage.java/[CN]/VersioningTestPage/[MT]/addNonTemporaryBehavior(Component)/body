{
  final Link<Void> addBehaviorLink=new Link<Void>("addBehaviorLink"){
    @Override public void onClick(){
      targetComponent.add(new AbstractBehavior(){
        /** 
 * @see org.apache.wicket.behavior.AbstractBehavior#isTemporary()
 */
        @Override public boolean isTemporary(){
          return false;
        }
      }
);
    }
  }
;
  add(addBehaviorLink);
}
