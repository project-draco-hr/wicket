{
  final NumberFormat numberFormat=getNumberFormat();
  final Number number=(Number)parse(numberFormat,value);
  if (number.doubleValue() < min) {
    throw newConversionException("Value cannot be less than " + min,value).setFormat(numberFormat);
  }
  if (number.doubleValue() > max) {
    throw newConversionException("Value cannot be greater than " + max,value).setFormat(numberFormat);
  }
  return number;
}
