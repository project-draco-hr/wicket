{
  if (settings.isEnabled()) {
    Counter counter=registry.counter(settings.getPrefix() + name + renderClassName(joinPoint));
    if (counterOperation == CounterOperation.INC) {
      counter.inc(value);
    }
 else {
      counter.dec(value);
    }
  }
  if (joinPoint != null) {
    return joinPoint.proceed();
  }
  return null;
}
