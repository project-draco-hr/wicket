{
  Class clazz=beanTypeCache == null ? null : (Class)beanTypeCache.get();
  if (clazz == null) {
    beanTypeCache=new WeakReference(clazz=Classes.resolveClass(beanTypeName));
    if (clazz == null) {
      throw new RuntimeException("SpringBeanLocator could not find class [" + beanTypeName + "] needed to locate the ["+ ((beanName != null) ? (beanName) : ("bean name not specified"))+ "] bean");
    }
  }
  return clazz;
}
