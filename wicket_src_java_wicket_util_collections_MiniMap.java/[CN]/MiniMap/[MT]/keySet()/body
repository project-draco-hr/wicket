{
  return new AbstractSet<K>(){
    @Override public Iterator<K> iterator(){
      return new Iterator<K>(){
        public boolean hasNext(){
          return i < size;
        }
        public K next(){
          i=nextKey(nextIndex(i));
          return keys[i];
        }
        public void remove(){
          keys[i]=null;
          values[i]=null;
          size--;
        }
        int i=-1;
      }
;
    }
    @Override public int size(){
      return size;
    }
  }
;
}
