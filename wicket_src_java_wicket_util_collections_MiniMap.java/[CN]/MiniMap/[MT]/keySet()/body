{
  return new AbstractSet<Object>(){
    @Override public Iterator<Object> iterator(){
      return new Iterator<Object>(){
        public boolean hasNext(){
          return i < size;
        }
        public Object next(){
          i=nextKey(nextIndex(i));
          return keys[i];
        }
        public void remove(){
          keys[i]=null;
          values[i]=null;
          size--;
        }
        int i=-1;
      }
;
    }
    @Override public int size(){
      return size;
    }
  }
;
}
