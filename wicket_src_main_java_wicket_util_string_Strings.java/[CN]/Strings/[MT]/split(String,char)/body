{
  if (s == null) {
    return new String[0];
  }
  final List<String> strings=new ArrayList<String>();
  int pos=0;
  while (true) {
    int next=s.indexOf(c,pos);
    if (next == -1) {
      strings.add(s.substring(pos));
      break;
    }
 else {
      strings.add(s.substring(pos,next));
    }
    pos=next + 1;
  }
  final String[] result=new String[strings.size()];
  strings.toArray(result);
  return result;
}
