{
  super.onConfigure();
  toolbars.configure();
  Boolean visible=toolbars.visitChildren(new IVisitor<Component,Boolean>(){
    @Override public void component(    Component object,    IVisit<Boolean> visit){
      object.configure();
      if (object.isVisible()) {
        visit.stop(Boolean.TRUE);
      }
 else {
        visit.dontGoDeeper();
      }
    }
  }
);
  if (visible == null) {
    visible=false;
  }
  setVisible(visible);
}
