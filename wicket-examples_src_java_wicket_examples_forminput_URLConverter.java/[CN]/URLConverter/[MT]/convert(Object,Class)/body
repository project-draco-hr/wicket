{
  if (value == null) {
    return null;
  }
  if (c == URL.class) {
    if (value.getClass() == URL.class) {
      return value;
    }
    try {
      return new URL(value.toString());
    }
 catch (    MalformedURLException e) {
      throw new ConversionException("'" + value + "' is not a valid URL");
    }
  }
  return value.toString();
}
