{
  Response response=headerResponse.getResponse();
  final CharSequence url=RequestCycle.get().urlFor(getResourceReference());
  response.write("<link rel=\"stylesheet\" type=\"text/css\" href=\"");
  response.write(url);
  if (media != null) {
    response.write("\" media=\"");
    response.write(media);
  }
  response.println("\" />");
}
