{
  super(parent,id);
  OrderByLink link=new OrderByLink(this,"orderByLink",property,stateLocator,OrderByLink.VoidCssProvider.getInstance()){
    private static final long serialVersionUID=1L;
    @Override protected void onSortChanged(){
      OrderByBorder.this.onSortChanged();
    }
  }
;
  onLinkCreated(link);
  add(new OrderByLink.CssModifier(link,cssProvider));
}
