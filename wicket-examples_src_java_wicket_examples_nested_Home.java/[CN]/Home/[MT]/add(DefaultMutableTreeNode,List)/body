{
  for (Iterator i=sub.iterator(); i.hasNext(); ) {
    Object o=i.next();
    if (o instanceof List) {
      DefaultMutableTreeNode child=new DefaultMutableTreeNode("<subtree>");
      parent.add(child);
      add(child,(List)o);
    }
 else {
      DefaultMutableTreeNode child=new DefaultMutableTreeNode(o);
      parent.add(child);
    }
  }
}
