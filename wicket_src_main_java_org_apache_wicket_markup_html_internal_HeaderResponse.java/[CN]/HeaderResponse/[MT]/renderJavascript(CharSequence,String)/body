{
  if (javascript == null) {
    throw new IllegalArgumentException("javascript cannot be null");
  }
  if (!closed) {
    List<String> token=Arrays.asList(javascript.toString(),id);
    if (wasRendered(token) == false) {
      JavaScriptUtils.writeJavascript(getResponse(),javascript,id);
      markRendered(token);
    }
  }
}
