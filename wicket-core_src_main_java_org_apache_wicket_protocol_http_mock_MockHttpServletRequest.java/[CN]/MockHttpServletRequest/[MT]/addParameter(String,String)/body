{
  if (value == null) {
    return;
  }
  String[] val=parameters.get(name);
  if (val == null) {
    parameters.put(name,new String[]{value});
  }
 else {
    String[] newval=new String[val.length + 1];
    System.arraycopy(val,0,newval,0,val.length);
    newval[val.length]=value;
    parameters.put(name,newval);
  }
}
