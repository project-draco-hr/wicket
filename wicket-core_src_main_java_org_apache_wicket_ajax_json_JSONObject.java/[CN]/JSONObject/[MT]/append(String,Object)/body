{
  testValidity(value);
  Object object=opt(key);
  if (object == null) {
    put(key,new JSONArray().put(value));
  }
 else   if (object instanceof JSONArray) {
    put(key,((JSONArray)object).put(value));
  }
 else {
    throw new JSONException("JSONObject[" + key + "] is not a JSONArray.");
  }
  return this;
}
