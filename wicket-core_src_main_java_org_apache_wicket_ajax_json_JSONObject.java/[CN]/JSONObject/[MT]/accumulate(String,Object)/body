{
  testValidity(value);
  Object object=opt(key);
  if (object == null) {
    put(key,value instanceof JSONArray ? new JSONArray().put(value) : value);
  }
 else   if (object instanceof JSONArray) {
    ((JSONArray)object).put(value);
  }
 else {
    put(key,new JSONArray().put(object).put(value));
  }
  return this;
}
