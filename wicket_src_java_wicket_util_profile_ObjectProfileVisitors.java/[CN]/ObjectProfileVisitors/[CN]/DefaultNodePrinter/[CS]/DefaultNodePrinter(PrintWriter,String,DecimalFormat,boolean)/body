{
  if (out == null) {
    throw new RuntimeException("null input: out");
  }
  m_out=out;
  m_indent=(indent != null) ? indent : "  ";
  if (format != null) {
    m_format=format;
  }
 else {
    m_format=(DecimalFormat)NumberFormat.getPercentInstance();
    m_format.setMaximumFractionDigits(1);
  }
  m_shortClassNames=shortClassNames;
}
