{
  TestPage testPage=new TestPage("unsupportedType");
  expectedException.expect(MarkupException.class);
  expectedException.expectMessage("Component [text] (path = [0:form:text]) must be applied to a tag" + " with [type] attribute matching any of [date, datetime, datetime-local, month, time, week], " + "not [unsupportedType]");
  tester.startPage(testPage);
}
