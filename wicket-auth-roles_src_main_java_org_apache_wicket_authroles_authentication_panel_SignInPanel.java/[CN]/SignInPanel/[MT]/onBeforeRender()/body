{
  if (isSignedIn() == false) {
    IAuthenticationStrategy authenticationStrategy=getApplication().getSecuritySettings().getAuthenticationStrategy();
    String[] data=authenticationStrategy.load();
    if ((data != null) && (data.length > 1)) {
      if (signIn(data[0],data[1])) {
        username=data[0];
        password=data[1];
        if (!continueToOriginalDestination()) {
          throw new RestartResponseException(getApplication().getSessionSettings().getPageFactory().newPage(getApplication().getHomePage()));
        }
      }
 else {
        authenticationStrategy.remove();
      }
    }
  }
  super.onBeforeRender();
}
