{
  try {
    init();
{
    }
{
    }
{
      int count=200;
      long time=System.currentTimeMillis();
      for (int i=0; i < count; i++) {
        parseAllSequentiallyWithMarkupCache();
      }
      long diff=System.currentTimeMillis() - time;
      long avgDuration=diff / count;
      long avgDurPerMarkup=avgDuration * 1000 / this.files.size();
      log.info("Time: " + diff + "ms; average per run: "+ avgDuration+ "ms; avg per markup: 0."+ avgDurPerMarkup+ "ms");
    }
  }
 catch (  Exception ex) {
    ex.printStackTrace();
  }
}
