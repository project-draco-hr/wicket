{
  String markdown=Resources.toString(Resources.getResource(MarkdownTest.class,"alert3.md"),Charset.forName("utf-8"));
  String html=Markdown.markdownToHtml(markdown);
  String expected=Resources.toString(Resources.getResource(MarkdownTest.class,"expected3.html"),Charset.forName("utf-8"));
  Assert.assertEquals(expected,html);
}
