{
  Object value=headers.get(name);
  if (value == null) {
    throw new WicketRuntimeException("Date header '" + name + "' is not set.");
  }
 else   if (value instanceof Long == false) {
    throw new WicketRuntimeException("Header '" + name + "' is not date type.");
  }
 else {
    return (Long)value;
  }
}
