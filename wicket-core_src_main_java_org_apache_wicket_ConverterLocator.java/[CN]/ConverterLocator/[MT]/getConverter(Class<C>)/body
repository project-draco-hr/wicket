{
  if (type == null) {
    @SuppressWarnings("unchecked") IConverter<C> converter=(IConverter<C>)new DefaultConverter<String>(String.class);
    return converter;
  }
  final IConverter<C> converter=get(type);
  if (converter == null) {
    return new DefaultConverter<C>(type);
  }
  return converter;
}
