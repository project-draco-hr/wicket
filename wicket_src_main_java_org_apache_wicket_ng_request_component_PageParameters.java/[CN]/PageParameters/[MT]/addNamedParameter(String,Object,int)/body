{
  Check.argumentNotNull(name,"name");
  Check.argumentNotNull(value,"value");
  if (namedParameters == null)   namedParameters=new ArrayList<Entry>(1);
  Entry entry=new Entry();
  entry.key=name;
  entry.value=value.toString();
  if (index == -1)   namedParameters.add(entry);
 else   namedParameters.add(index,entry);
}
