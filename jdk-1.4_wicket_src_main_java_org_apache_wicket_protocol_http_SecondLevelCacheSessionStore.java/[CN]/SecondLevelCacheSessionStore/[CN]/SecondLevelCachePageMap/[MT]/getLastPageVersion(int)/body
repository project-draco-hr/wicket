{
  int versionNumber=-1;
  PageVersions pv=null;
  for (int index=pageVersions.size() - 1; index >= 0; --index) {
    if (((PageVersions)pageVersions.get(index)).pageid == id) {
      pv=(PageVersions)pageVersions.get(index);
      versionNumber=pv.versionid;
      break;
    }
  }
  return versionNumber;
}
