{
  if (sessionStore == null) {
    Application app=Application.exists() ? Application.get() : null;
    if (app != null) {
      sessionStore=(SecondLevelCacheSessionStore)app.getSessionStore();
    }
  }
  if (sessionStore != null) {
    return sessionStore.getStore();
  }
 else {
    return null;
  }
}
