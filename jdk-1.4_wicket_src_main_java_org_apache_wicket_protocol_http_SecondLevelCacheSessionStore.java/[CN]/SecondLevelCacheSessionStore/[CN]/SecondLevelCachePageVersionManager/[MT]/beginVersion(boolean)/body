{
  if (versionStarted) {
    return;
  }
  versionStarted=true;
  if (!mergeVersion) {
    currentVersionNumber=newVersion();
    lastAjaxVersionNumber=currentAjaxVersionNumber;
    currentAjaxVersionNumber=0;
  }
 else {
    if (RequestCycle.get().getRequest() instanceof WebRequest && ((WebRequest)RequestCycle.get().getRequest()).isAjax()) {
      currentAjaxVersionNumber++;
    }
  }
}
