  assertEquals(1,page.getNrPageDetachModelsCalls());
  assertEquals(1,page.getNrPageOnDetachCalls());
  assertEquals(1,page.getNrModelDetachCalls());
  assertEquals(1,page.getNrAjaxBehaviorDetachModelCalls());
}
