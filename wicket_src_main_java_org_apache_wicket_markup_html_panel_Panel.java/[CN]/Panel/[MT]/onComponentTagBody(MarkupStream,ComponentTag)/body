{
  renderAssociatedMarkup(PANEL,"Markup for a panel component has to contain part '<wicket:panel>'");
  if (markupStream.getPreviousTag().isOpen()) {
    markupStream.skipRawMarkup();
    if (markupStream.get().closes(openTag) == false) {
      throw new MarkupException(markupStream,"close tag not found for tag: " + openTag.toString() + ". Component: "+ this.toString());
    }
  }
}
