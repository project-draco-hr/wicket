{
  File sessionDir=new File(defaultWorkDir,sessionId);
  if (sessionDir.exists()) {
    File[] files=sessionDir.listFiles();
    if (files != null) {
      for (int i=0; i < files.length; i++) {
        files[i].delete();
      }
    }
    if (!sessionDir.delete()) {
      sessionDir.deleteOnExit();
    }
  }
}
