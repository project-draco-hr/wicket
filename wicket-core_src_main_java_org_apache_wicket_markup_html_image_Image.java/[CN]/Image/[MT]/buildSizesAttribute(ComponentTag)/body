{
  if (this.sizes == null) {
    return;
  }
  String sizes="";
  for (  String size : this.sizes) {
    sizes+=size + ",";
  }
  int lastIndexOf=sizes.lastIndexOf(",");
  if (lastIndexOf != -1) {
    sizes=sizes.substring(0,lastIndexOf);
  }
  if (!"".equals(sizes)) {
    tag.put("sizes",sizes);
  }
}
