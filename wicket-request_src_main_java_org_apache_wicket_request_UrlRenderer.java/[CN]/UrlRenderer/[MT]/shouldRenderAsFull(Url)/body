{
  if (!Strings.isEmpty(url.getProtocol()) && !url.getProtocol().equals(request.getClientUrl().getProtocol())) {
    return true;
  }
  if (!Strings.isEmpty(url.getHost()) && !url.getHost().equals(request.getClientUrl().getHost())) {
    return true;
  }
  if ((url.getPort() != null) && !url.getPort().equals(request.getClientUrl().getPort())) {
    return true;
  }
  return false;
}
