{
  if (inputStreams != null) {
    for (Iterator inputStreamsIterator=inputStreams.iterator(); inputStreamsIterator.hasNext(); ) {
      InputStream inputStream=(InputStream)inputStreamsIterator.next();
      try {
        inputStream.close();
      }
 catch (      IOException e) {
      }
    }
    inputStreams=null;
  }
}
